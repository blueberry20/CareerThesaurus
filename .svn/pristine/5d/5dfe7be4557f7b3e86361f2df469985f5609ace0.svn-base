    ViewBag.Title = "Find a School";
    Layout = "~/Views/Shared/_KioskLayout.cshtml";
}

<br />


<div style="margin-left: 0px;">
    <h1>Find a School</h1>

    <div style="margin: @(ViewBag.IsMobileDevice ? "40px" : "60px") 0 0 125px; position: relative;">
        <span style="font-size: 14pt;">Enter your ZIP code:</span>
        <!--<input id="zip" type="text" style="position: absolute; left: 280px; top: -12px; width: 57px; " />-->
        @if (ViewBag.IsMobileDevice == false)
        {
        <div id="zip" keyboardtype="numeric" prompt="Enter your ZIP code:" style="position: absolute; left: 280px; top: -10px; width: 57px; padding: 10px; height: 23px; font-size: 14pt; border: thin solid rgba(255,255,255,1);"></div>
        }
        else
        {
        <input type="text" id="zip" style="position: absolute; left: 280px; top: -10px; width: 57px; padding: 10px; height: 23px; font-size: 14pt; border: thin solid rgba(255,255,255,1);"/>
        }
    </div>

    <div style="margin: 40px 0 0 125px; position: relative;">
        <span style="font-size: 14pt;">High School Graduation year:</span>
        @if (!ViewBag.IsMobileDevice)
        {
        <div id="gradyear" style="position: absolute; left: 280px; top: -10px; width: 57px; padding: 10px; height: 23px; font-size: 14pt; border: thin solid #fff;">
            <img src="/content/images/kiosk/icons/arrow_4.png" style="margin-left: 18px;"/>
        </div>
        <div id="gradyearoptionsconnector" style="position: absolute; left: 359px; top: 11px; width: 261px; height: 0px; border: thin solid #fff; display: none;" class="selectorcontrols shadow20"></div>
        }
        else
        {
            <select id="gradyear" style="position: absolute; left: 280px; top: -10px; width: 120px; padding: 10px; font-size: 14pt; border: thin solid #fff; background-color: white; color: black;">
                <option value=""></option>
            </select>
        }
    </div>

    <div id="education_level_container" style="margin: 40px 0 0 125px; position: relative;">
