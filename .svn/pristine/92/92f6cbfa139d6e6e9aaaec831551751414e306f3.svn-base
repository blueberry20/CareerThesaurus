<script>
    function document_ready() {
        //activate bootstrap scrollspy
        $('body').scrollspy({
            offset: 300
        });

        //redirect to career description page
        $('.listing').click(function () {
            location.href = '@Url.Content("~/Career")';
        })


        //Mobile version. Activate nicescroll
        $(".scrollableDiv").niceScroll({ cursoropacitymin: 0.9 });


        //Show mobile tags overlay on click of icon
        $('.tagsIcon').click(function () {

            //if closed
            if ($('#mobileCareerTags').is(':visible')) {
                $('#mobileCareerTags').hide();
                $('.navbar-toggle').css('display', 'block'); //menu icon
                $('.filterIcon').css('display', 'block');
                $('.tagsIcon p').text("Tags");
            } else { //if open
                $('#mobileCareerTags').show();
                $('.navbar-toggle').css('display', 'none');
                $('.filterIcon').css('display', 'none');
                $('.tagsIcon p').text("Done");
            };
        });

        /*show advanced search on click of icon*/
        $('.filterIcon').click(function () {
            //if closed
            if ($('#mobileCareerFilter').is(':visible')) {
                $('#mobileCareerFilter').hide();
                $('.navbar-toggle').css('display', 'block');
                $('.tagsIcon').css('display', 'block');
                $('.filterIcon i').removeClass("icon-cancel").addClass("icon-filter");
            } else {//if open
                $('#mobileCareerFilter').show();
                $('.navbar-toggle').css('display', 'none');
                $('.tagsIcon').css('display', 'none');
                $('.filterIcon i').removeClass("icon-filter").addClass("icon-cancel");
            };
        });

        //Tags mobile page. On click of keyword, add to search query
        $(".mobileKeyword").click(function () {
            //adds class selected to the element that was clicked
            $(this).addClass("selected");
            //create a duplicate of the element that was clicked
            var cloneKeyword = $(this).clone();
            cloneKeyword.addClass('tag');

            cloneKeyword.click(function () {
                $(this).remove();
            });

            //place the duplicate inside the top div after last tag and add close glyphicon
            cloneKeyword.insertAfter('#mobileCareerTags .searchQueryDiv a:last-child').removeClass('selected').removeClass('mobileKeyword').append('<span class="glyphicon glyphicon-remove"></span>');
        });

        //delete keyword on click in the query section
        $('.tag').click(function () {
            $(this).remove();
        });
    };
</script>