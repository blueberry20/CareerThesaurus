

* {
    font-family:'open sans';
}

.container {
    padding:0;
}

.pageContainer {
    min-height:100%;
}

.blue {
    background-color:rgb(23, 184, 214);
}

.section {
    min-height:500px;
    padding:100px 15px;
    background-position: center top;
    margin:0;
    background-size:cover;
}

.jumbotron h1, .jumbotron .h1 {
    color:#fff !important;
    text-align:center;
    font-size:50px;
}

.enterBtn {
    border: 2px solid #fff;
    border-radius: 15px;
    width: 300px;
    height: 70px;
    line-height: 66px;
    padding: 0;
    color: #fff;
    font-size: 23px;
    font-family: 'open sans';
    text-transform: uppercase;
    position:relative;
    margin-top:100px;
}

.enterBtn.blue:hover, .enterBtn.blue:focus, .enterBtn.blue:active {
    background-color: rgb(79, 206, 229);
}

.enterBtn:hover, .enterBtn:focus, .enterBtn:active {
    background-color: rgb(230,230,230);
    background-color: rgba(255,255,255, 0.3);
    color:#fff;
}

.enterBtn .fa-angle-right {
    position:absolute;
    top:50%;
    font-size:26px;
    margin-top:-13px;
    margin-left:15px;
}

.jumbotron {
    position:relative;
    margin-bottom:0;
}

.jumbotron p {
    color: #fff !important;
    font-size: 28px;
    margin: 40px 20px;
    text-align:center;
}

.section p {
    color: #fff !important;
    font-size: 26px;
    margin: 40px 20px;
    text-align:center;
    font-weight:300;
}

.blackboard {
    background-image: url(/Content/images/blackboard.jpg);
}

h2 {
    margin: 0;
    background-color: #000;
    padding: 40px;
    color: #fff;
    text-align: center;
    font-size: 52px;
    font-family: 'open sans condensed';
    font-weight:600;
    text-transform:uppercase;
}

.row.combinedLogoRow {
    text-align: center;
    margin:0;
}

.combinedLogoRow .logoImg {
    display: inline;
    height: 100px;
}

.combinedLogoRow .logoText {
    display: inline;
    margin-left:50px;
}

.sectionTwo {
    background-image: url(/Content/images/students_comp.jpg);
    background-position: center center;
    min-height:600px;
}

.sectionTwo .enterBtn {
    margin-top:250px;
}

h3 {
    text-align: center;
    text-transform: uppercase;
    color: #fff;
    text-decoration: underline;
}

.sectionFour p, .sectionTwelve p {
    font-weight: 600;
    font-size: 60px;
    text-transform: uppercase;
    font-family: open sans condensed;
    margin:10px;
}

.sectionFour p:nth-child(1), .sectionFour p:nth-child(2), .sectionTwelve p:nth-child(1) {
    color: rgb(14, 67, 130) !important;
}

.sectionFour p:nth-child(3), .sectionFour p:nth-child(4), .sectionFour p:nth-child(5), .sectionTwelve p:nth-child(2), .sectionTwelve p:nth-child(3), .sectionTwelve p:nth-child(4) {
    color:rgb(10, 114, 163) !important;
}

.sectionFive {
    background-image: url(/Content/images/student_arrows.jpg);
    background-position: center center;
    min-height:800px;
}

.sectionFive .enterBtn {
    margin-top: 50%;
}

.section.sectionFooter {
    background-color: #000;
    text-align: center;
}



/*analytics index code*/
.leftColumn {
    padding: 0 5px 0 0;
}

.rightColumn {
    padding: 0 0 0 5px;
}

.graphicon {
    height: 76px;
    margin-right: 20px;
    position: absolute;
    top: 50%;
    margin-top: -37px;
}

.graphicon.special {
    height:100px;
}

.reportWrapper {
    display:table-cell;
    vertical-align:middle;
    padding-left:90px;
    padding-right:10px;
    position:absolute;
    top:50%;
}

.reportDiv {
    padding: 30px;
    border: 1px solid #D2FAFB;
    margin: 0 0 10px;
    cursor:pointer;
    /*box-shadow: rgba(0,0,0,0.2) 10px 10px 20px ;*/
    position:relative;
    min-height:170px;
}

.reportDiv:hover {
    /*-moz-box-shadow: 0 0 5px #888;
    -webkit-box-shadow: 0 0 5px#888;
    box-shadow: 0 0 5px #888;*/
    background-color:#E5F5FA;
}

.reportDiv h3 {
    text-align: left;
}

.reportDiv p {
    font-weight:300;
    font-size:19px;
    margin:10px 0 0;
}

.iconBox {
    float: left;
    height: 120px;
    line-height: 120px;
}

.wrapper {
    margin-bottom: 0;
    padding: 2em;
}
    
.wrapper.style1 {
    background:#ffffff;
    padding-bottom:2em;    
}
.wrapper.style2 {
    background:#fff;
    padding:0; 
}  


#canvas_container {
    position: fixed;
    top: 0;
    left: 0;
    height: 755px;
    width: 610px;
}
/*end*/

.img-responsive {
    display: inline-block;
}


.sectionSeven p {
    font-weight:400
}

.sectionSeven ul {
    width:60%;
    margin: 0 auto;
}

.sectionSeven li {
    color: #fff;
    font-size: 26px;
    text-align: left;
    font-weight: 300;
}

.sectionTwelve {
    background-color: rgb(243, 245, 246);
}
.sectionFooter img {
    display:inline-block;
    text-align:center;
    margin-bottom:50px;
}


 .sectionFooter p {
     margin-bottom:0;
 }

 .sectionFooter .row {
    padding: 0;
    margin: 0;
}

 .underline  {
     text-decoration: underline;
     font-family:'open sans condensed';
 }



 /*MIN WIDTH*/
 @media only screen and (min-width: 0px) and (max-width:1200px) {
    .container {
        width: initial; 
    }
 }


 /*MAX WIDTH*/
@media only screen and (max-width:992px) {
    .jumbotron h1, .jumbotron .h1, .sectionThirteen h2 {
        font-size: 40px;
    }

    .logoText {
        width:80%;
    }

    .section {
        min-height:400px;
    }

    .combinedLogoRow .logoText {
        margin-left: 30px;
    }

    .combinedLogoRow .logoImg {
        height: 80px;
    }

    .sectionSeven ul {
        width: 70%;
    }

    .sectionTen p {
        font-size: 50px;
    }

}

@media only screen and (max-width:768px) {
    .jumbotron h1, .jumbotron .h1, .sectionThirteen h2 {
        font-size: 30px;
    }

    .combinedLogoRow .logoImg {
        height: 60px;
    }

    .combinedLogoRow .logoText {
        margin-left: 20px;
    }

    h2 {
        font-size: 32px;
        padding: 20px;
    }

    h3 {
        font-size:30px;
        padding:30px 20px;
    }

    .section p {
        font-size:20px;
    }

    .sectionFour p, .sectionTwelve p {
        font-size: 40px;
    }

    .sectionSeven p {
        font-size: 22px;
    }

    .sectionSeven li {
        font-size: 22px;
    }


    .enterBtn {
        width:250px;
        font-size:18px;
    }

    .enterBtn .fa-angle-right {
        font-size: 20px;
        margin-top: -10px;
    }

    .sectionTwelve .enterBtn {
        font-size: 15px;
    }

    .logoText {
        width:80%;
    }

    .sectionFooter p {
        font-size:14px;
    }
}

@media only screen and (max-width:500px) {
    .combinedLogoRow .logoImg {
        height: 40px;
    }

    .jumbotron h1, .jumbotron .h1, .sectionThirteen h2 {
        font-size: 20px;
    }

    h2 {
        font-size: 22px;
        padding:15px;
    }

    .enterBtn {
        margin:20px 0;
        width:200px;
        height:50px;
        line-height:50px;
        font-size:14px;
    }

    h3 {
       font-size: 17px;
        padding: 10px;
        margin: 0;
    }

    .section {
        padding: 20px 10px;
        min-height: 300px;
    }

    .section p {
        font-size: 14px;
        margin:20px 15px;
    }

    .sectionOne .enterBtn {
        margin-top:50px;
    }

    .sectionTwo .enterBtn {
        margin-top: 190px;
    }

    .sectionFour p, .sectionTwelve p {
         font-size: 20px;
    }

    .sectionFive .enterBtn {
        margin-top: 60%;
    }

    .sectionSeven ul {
        width:100%;
    }

    .sectionSeven li {
        font-size: 14px;
    }

    .sectionFooter p {
        font-size: 14px;
        margin: 40px 0;
    }

}