/* PROFESSION PAGE
-------------------------------------------------- */
#professionPage {
    padding-top: 50px;
    background-color: #EBF2F2;
    background-color: #F7FBFB;
}

#professionPage .container .row {
    margin: 0;
}

#professionPage h1 {
    margin-top: 0;
    font-weight: 600;
}

#professionPage h3 {
    font-weight: 600;
    margin:0 0 10px;
}

#professionPage h4, #professionPage h4 span {
    font-size: 20px;
    margin: 20px 0;
    font-weight: 600;
}



table {
    margin: 20px 0;
    /*background-color: #F8FDFE;*/
}

.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
    padding: 4px 4px;
    line-height: 1.428571429;
    vertical-align: top;
    border-top: 1px solid #B1C4CA;
    /*font-family: 'open sans condensed';*/
    font-size: 14px;
}

.table > thead > tr > th {
    border-bottom: none;
}
.table > tbody > tr:first-child > td {
    border-top: none;
}

.highestCell {
    background-color: #BCFFE5;
    padding: 0 3px;
    font-weight: 600;
}

.lowestCell {
    background-color: #FFCFCF;
    padding: 0 3px;
    font-weight:600;
}

#cbnSchools {
    padding-top: 0;
}

#vantageMedia {
    padding-bottom:0;
    margin-bottom: 0;
    display:none;
}

.mapicon {
    height: 50px;
    margin-right: 10px;
}

