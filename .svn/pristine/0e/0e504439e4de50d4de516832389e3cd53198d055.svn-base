/*****************************career thesaurus desktop version***********************/

#careerThesaurus {
    background-color: #f0f0f0;
    height: 450px;
    padding: 8px;
    position: relative;
}


#circleSmallLeft1, #circleSmallLeft2, #circleSmallLeft3 {
    height: 70px;
    width: 70px;
    border-radius: 50%;
    border: 1px solid #F5AB6A;
    background-color: #FFE4C4;
    position: absolute;
    padding: 20px 0;
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    -ms-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
    cursor: pointer;
}

#circleSmallLeft1 a, #circleSmallLeft2 a, #circleSmallLeft3 a {
    color: #000;
    font-size: 14px;
    width: 150px;
    display: block;
    text-shadow: 2px 2px #fff;
    position: absolute;
    right: 0;
    top: 35%;
    text-decoration: none;
    font-weight: 600;
}

#circleSmallLeft1 {
    top: 19%;
    left: 13%;
}

.no-touch #circleSmallLeft1:hover {
    top: 10%;
    left: 5%;
}

#circleSmallLeft2 {
    z-index: 2;
    top: 27%;
    left: 7%;
}

.no-touch #circleSmallLeft2:hover {
    left: -5%;
}

#circleSmallLeft3 {
    top: 35%;
    left: 13%;
}

.no-touch #circleSmallLeft3:hover {
    top: 43%;
    left: 5%;
}


#circleBig {
    height: 110px;
    width: 110px;
    border-radius: 50%;
    border: 10px solid #5bc0de;
    position: absolute;
    top: 22%;
    left: 16%;
    font-weight: 600;
    text-align: center;
    font-size: 18px;
    padding: 20px 0;
    z-index: 2;
    background-color: #5bc0de;
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    -ms-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
}

    #circleBig p {
        display: inline;
    }

#title1 {
    position: absolute;
    top: 2%;
    right: 0;
    font-weight: 600;
    font-size: 20px;
    width: 150px;
}


#arrowRight1 {
    display: none;
}

#arrowRight2 {
    font-size: 50px;
    position: absolute;
    right: 37%;
    top: 27%;
    color: #c9c9c9;
}

#circleSmallRight1, #circleSmallRight2, #circleSmallRight3 {
    height: 70px;
    width: 70px;
    border-radius: 50%;
    border: 5px solid #e0e0e0;
    position: absolute;
    right: 20%;
    color: #0080FF;
    font-weight: 600;
    padding: 20px 0;
    background-color: #fff;
    font-size: 13px;
}

#circleSmallRight1 a, #circleSmallRight2 a, #circleSmallRight3 a {
    z-index: 100;
    width: 150px;
    display: block;
    color: #0080FF;
}

#circleSmallRight1 {
    top: 15%;
}

#circleSmallRight2 {
    top: 27%;
    z-index: 2;
}

#circleSmallRight3 {
    top: 40%;
}

.icon-down-bold {
    font-size: 50px;
    position: absolute;
    left: 22%;
    top: 42%;
    color: #c9c9c9;
}

#title2 {
    position: absolute;
    top: 54%;
    left: 20%;
    font-weight: 600;
    color: #000;
}

#title3 {
    position: absolute;
    top: 58%;
    left: 20%;
    font-style: italic;
    font-weight: 600;
}

#circleSmallBottom1, #circleSmallBottom2, #circleSmallBottom3 {
    height: 50px;
    width: 50px;
    border-radius: 50%;
    border: 5px solid #e0e0e0;
    position: absolute;
    left: 25%;
    font-weight: 600;
    padding: 10px 0;
    background-color: #fff;
    font-size: 13px;
}

#circleSmallBottom1 a, #circleSmallBottom2 a, #circleSmallBottom3 a {
    z-index: 100;
    width: 200px;
    display: block;
    color: #0080FF;
}

#circleSmallBottom1 {
    top: 62%;
}

#circleSmallBottom2 {
    top: 70%;
    z-index: 2;
}

#circleSmallBottom3 {
    top: 78%;
}

#thesaurusBtn {
    position: absolute;
    right: 8px;
    bottom: 3%;
}

