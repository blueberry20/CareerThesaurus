}

#likeCareerBtn.chosenBtn:hover, #likeCareerBtn.chosenBtn:focus, #likeCareerBtn.chosenBtn:active {
    background-color:#2FBC8F;
    color:#000;
}


.navigateCareersRow .btn:hover, .navigateCareersRow .btn:focus, .navigateCareersRow .btn:active {
    color: #064E97;
    text-decoration:underline;
}


