.pageContainer {
    background-attachment:inherit;
    background-image:url("../../images/hex/hex_bg14.jpg");
    position:relative;
}

.pageContainer .container {
       background-color:#fff;
       height:auto;
       padding-bottom:3em;
       padding-top:2em;
}
p {
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 600;
    font-size: 20px;
    line-height: 23px;
    height: 31px;
    padding: 0;

}


.graphicon {
   height:100px;

}
.graphicon.special {
      height:100px;
}
.reportDiv {
    padding: 30px;
    border: 1px solid #Ffffff;
    margin: 20px 0;
    cursor:pointer;
    box-shadow: rgba(0,0,0,0.2) 10px 10px 20px ;
 
  
}

.reportDiv h3 :hover {
    -moz-box-shadow: 0 0 5px #888;
    -webkit-box-shadow: 0 0 5px#888;
    box-shadow: 0 0 5px #888;
}

	.wrapper {
