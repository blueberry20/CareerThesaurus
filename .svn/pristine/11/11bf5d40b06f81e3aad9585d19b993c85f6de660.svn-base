<script>
    function setlocationzip() {
        var zip = $('#indeedzipcode').val();
        if (zip.length != 5) {
            alert('Invalid zip code entered');
            $('#indeedzipcode').focus();
            $('#indeedzipcode').select();
        }
        setCookie("zip", zip, 365);

        $('#jobslocation').html('Showing jobs near zip <b>' + zip + '</b>');

        findjobs();
    }
    
</script>

