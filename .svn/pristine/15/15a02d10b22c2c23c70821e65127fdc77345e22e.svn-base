/*version 4 of date display. display:table*/
#dateInfoV4 {
display:inline-block;   
text-align:center;
font-weight:600;
color:#000;
float:right;
width:200px;
}

#dateInfoV4 p {
line-height:14px;
font-size:14px;   
margin:0;
padding:0;
display:inline-block;
vertical-align:top;
}

#dateInfoV4 .dateTable {
display:table;   
text-align:center;
border-spacing:3px;
vertical-align:top;
}

#dateInfoV4 .hrDigit, #dateInfoV4 .minDigit, #dateInfoV4 .secDigit {
display:table-cell; 
width:30px;  
border: 1px solid #000;
border-radius:4px;
background-color:#fff;
color:#000;
}


