display:table;   
text-align:center;
position:relative;
}

.tableRow {
display:table-row;   
position:absolute;
}

.hrDigit, .minDigit, .secDigit, .hr, .min, .sec {
display:table-cell; 
width:30px;  
margin: 0 2px;
border: 1px solid #fff;
}

.hrDigit, .minDigit, .secDigit {
background-color: #000;   
color:#fff;
}

.secDigit {
background-color: rgb(213,35,95);   
color:#fff;
}


#testJob .dateHolder {
    display:inline-block;
    text-align:center;
}

#testJob .dateTable {
    width:200px;
    position:relative;
}

#testJob .tableRow {
    position:absolute;
    left:0;
    top:0;
    display:block;
}

.hrDigit, .minDigit, .secDigit {
background-color: #000;
color: #fff;
top: 0;
position:absolute;
width:30px;
}

.hr, .min, .sec {
top: 18px;
background-color: #686565;
width: 22px;
position: absolute;
border: 1px solid #666;
font-size: 7pt;
color: #f0f0f0;
line-height: 0.9;
font-family:'small fonts';
margin:0 4px;
padding-bottom: 2px !important;
line-height: 7pt;
}

.hrDigit {
left: 0;
}

.minDigit {
left: 30px;
}

.secDigit {
left: 60px;
background-color: rgb(213,35,95);
}

.hr {
    left:2px;
}

.min {
left: 32px;
}

.sec {
    left: 62px;
}

/*if no hour is displayed, change position of timestamp to be aligned left*/
.minutesPosted .hrDigit {
    display:none;
}

.minutesPosted .minDigit {
    left:0;
}

.minutesPosted .secDigit {
    left:30px;
}

.minutesPosted .hr {
    display:none;
}

.minutesPosted .min {
    left:3px;
}

.minutesPosted .sec {
    left:33px;
}

