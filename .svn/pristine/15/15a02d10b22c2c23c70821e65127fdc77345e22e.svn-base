/*add cancel icon to input field*/
.clearable{
    position:relative;
}
.data_field{
    padding-right:17px; /* add space for the 'x' icon*/
}
span.icon_clear{
    position: absolute;
    right: 70px;
    top: 50%;
    margin-top: -10px;
    display: none;
    cursor: pointer;
    /* font: bold 1em sans-serif; */
    color: #C4BFBF;
    border: 1px solid #C4BFBF;
    border-radius: 50%;
    display:none;
    }

    span.icon_clear:hover{
        color:#f52;
    }

    .clearable:focus .icon_clear {
        display:inline-block;
    }

