.annotation {
    color: #1C8791;
    font-weight: 600;
    font-size: 24px;
    font-style: italic;
}

.arrowRight {
    position: absolute;
    right: 0;
    top: 50%;
    font-size: 24px;
    margin-top: -12px;
    line-height:normal;
    margin-right:7px;
}

.tir {
    font-size:20px !important;
    margin-top: -10px !important;
}

.iconExplanation {
    background-color:#E7F6F9;
    padding:10px;
    line-height:2em;
}

.iconExplanation .tir {
    width:20px;
    text-align:center;
}

.blueStripesBtn {
    display: inline-block;
    line-height: 50px;
    padding: 0 10px;
    color: #000;
    background-color: transparent;
    border: none;
    height: 50px;
    text-transform: uppercase;
    font-weight: 600;
    font-size: 17px;
    position: relative;
    padding-right: 30px;
    background-image:url(/Content/images/stripespattern8.png);
    margin:10px 0;
}

.no-touch .blueStripesBtn:hover, .blueStripesBtn:focus, .blueStripesBtn:active {
    background-image:url(/Content/images/stripespattern6.png);
    color:#000;
    text-decoration:none;
}

.careersByTRaitBtn .arrowRight {
    font-size: 17px;
    margin-top: -12px;
}

.reportDiv .extraMargin {
