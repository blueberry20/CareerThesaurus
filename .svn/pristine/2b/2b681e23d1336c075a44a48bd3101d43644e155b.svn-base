.pageContainer {
    padding: 50px 0 0;
}

/*.whiteBgPage .container {
    background-color: #fff;
    padding: 20px 30px 30px;
}*/

#studentReportPage h1 {
    text-align:left;
    display:block;
    margin:40px 0;
    font-size:38px;
}

#studentReportPage.lockedMode h1 {
    margin:0;
    padding:20px 0;
}

#studentReportPage.lockedMode .container {
    padding:15px;
}

#studentReportPage.lockedMode .reportWrapper {
    padding:0 5px;
}

#studentReportPage.lockedMode .rightColumn {
    padding:0;
}

#studentReportPage.lockedMode .printLink {
    right:10px;
}

#studentReportPage.lockedMode .noResultsError {
    margin:10px 0;
}

.pageContainer h2 {
    margin: 20px 0;
    text-align: left;
    color: #000;
    font-weight: 600;
    font-family: 'open sans';
    font-size: 24px;
}

.reportDiv h2 {
    margin: 0;
    text-transform: none;
    font-size: 30px;
    cursor: pointer;
    background-color: #E4F9FB;
    padding: 15px;
    position:relative;
    padding-right:70px;
}

#studentReportPage h2 span {
    vertical-align:middle;
}

h3 {
    text-align: left;
    margin: 30px 0 10px;
    font-weight:300;
    font-size:22px;
}

.reportDiv {
    /*background-color: #FAFDFE;*/
    margin: 20px 0;
}

.reportSection {
    padding-left: 15px;
    /*background-color: #F8FCFC;*/
    padding: 20px;
}


.arrowDown, .arrowUp {
    display: inline-block;
    position: static;
    /*background-color: #D2F4F9;*/
    line-height: 30px;
    height: 30px;
    float: right; 
    vertical-align: middle;
    font-size:57px;   
    -webkit-transition:all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    transition:all 0.5s ease-in-out;
    position: absolute;
    right: 20px;
    top: 50%;
    margin-top: -15px;
}

.arrowDown {
    display:none;
}

/*.traitsDescription p {
    font-size:18px;
}*/

.cell a {
    color: #000;
    height: 100%;
    width: 100%;
    display: inline-block;
}

.no-touch .cell a:hover, .cell a:focus, .cell a:active {
    text-decoration:none;
    color: rgb(23, 184, 214);
}

.cell .firstLine {
    font-family: 'open sans condensed';
    display: block;
    font-size: 34px;
    font-weight: 700;
    line-height: 34px;
    text-align: center;
    margin-top: 42px;
}

.cell .secondLine {
    font-family: 'open sans condensed';
    display: block;
    font-size: 22px;
    font-weight: 400;
    line-height: 22px;
    text-align: center;
}

.secondLineInversed, .secondLine.inversed {
    font-family: 'open sans condensed';
    display: block;
    font-size: 22px;
    font-weight: 400;
    line-height: 22px;
    text-align: center;
    margin-top:40px;
}

.firstLineInversed, .firstLine.inversed {
    font-family: 'open sans condensed';
    display: block;
    font-size: 34px;
    font-weight: 700;
    line-height: 34px;
    text-align: center;
    margin-top:0;
}

.cell {
    border: 1px dashed rgb(147, 229, 237);
    height: 130px;
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}

.cell p {
    margin-top: 55px;
}



.reportWrap {
    position:relative;
}

.reportLink {
    margin-top: 20px;
    background-color: #000;
    color: #fff;
    padding: 5px;
}

.major {
    margin: 5px 0;
    color: #999;
    font-size: 13px;
}

.no-touch .reportLink:hover, .reportLink:focus, .reportLink:active {
    color:#fff;
    text-decoration:none;
    background-color: rgb(18, 157, 183);
} 

.printLink {
    margin-top: 5px;
    cursor: pointer;
    width: 150px;
    position: absolute;
    top: 0;
    right: 5px;
}


.printLink img {
    height: 30px;
    margin-right: 10px;
}

.reportCard {
    color: #C0B9B9;
    font-weight: 700;
    font-size: 36px;
    text-transform: uppercase;
    position: absolute;
    top: -50px;
    right: 0;
}

.career {
    border-bottom: 1px solid rgb(23, 184, 214);
  
}

.no-touch .career:hover {
    /*background-image: url(/Content/images/stripespattern8.png);*/
    background-repeat:repeat;  
    text-decoration:none;
    color:#000;
}

.traitsDescription {
    opacity:0;
}

.traitsDescription.notViewed {
    opacity:0;
    position:relative;
    left:100px;
}

.traitsDescription.viewed {
    opacity:1;
        -webkit-transition:all 1s ease-in-out;
    -moz-transition: all 1s ease-in-out;
    -o-transition: all 1s ease-in-out;
    transition:all 1s ease-in-out;
    left:0;
}

