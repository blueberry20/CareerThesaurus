<script>
    var winWidth,
    winHeight,
    isDeviceMobile = "@ViewBag.IsMobileDevice".toLowerCase() == 'true' ? true : false,
    isiPad = "@ViewBag.IsiPad".toLowerCase() == 'true' ? true : false;

    $(document).ready(function () {
        displaySize();
        $(window).resize(function () {
            displaySize();
        });
        window.onorientationchange = function (event) {
            displaySize();
        }
    });
    function displaySize() {
        if (isiPad) {
            if (Math.abs(window.orientation) === 90) {
                winWidth = screen.height;
                winHeight = screen.width - 97;
                //landscape
            } else {
                //portrait
                winWidth = document.documentElement.clientWidth;
                winHeight = document.documentElement.clientHeight;
            }
        } else {
            winWidth = document.documentElement.clientWidth;
            winHeight = document.documentElement.clientHeight;
        }
        if (winHeight > winWidth) {
            $("#background").width(winHeight).height(winHeight).css({ 'left': Math.floor((winWidth - winHeight) / 2), 'right': Math.floor((winWidth - winHeight) / 2), 'top': 0, 'bottom': 0 });
        } else {
            $("#background").width(winWidth).height(winWidth).css({ 'top': Math.floor((winHeight - winWidth) / 2), 'bottom': Math.floor((winHeight - winWidth) / 2), 'left': 0, 'right': 0 });
        }
    } // END display size
</script>
