.title{
    font-family: 'Open Sans', Sans-Serif;
    text-transform: uppercase;
    font-weight: 500;
    color: #000000;
    font-size:16px;    
}


.barWraper {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: uppercase;
    font-weight: 600;
    color: #000000;
    font-size: 18px !important;
}

.bar {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: uppercase;
    font-weight: 600;
    background-color: #3FE0FF;
    display: block;
    height: 26px;
    width: 0;
    position: relative;
    text-align: right;
}


.span {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: none;
    font-weight: 600;
    color: #000000;
    font-size: 16px !important;
    float:right !important;
}


#control-label.special {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: none;
    font-weight: 600;
    color: #000000;
    font-size: 14px !important;
    float:right!important;
}

.reportHeader{
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 600;
    color: #000000;
    position:relative;
    font-size:34px;
    margin-bottom: 7px;
    width:100%;
    letter-spacing: -1px;
    text-align:center;
    text-transform:none;
}

.reportHeader.special{
    font-family: 'Open Sans', Sans-Serif;
    line-height:100px;
    height:80px;
    letter-spacing: -1px;
    text-align:center;
    text-transform:none !important;
}

#rowcolor {
    background: #4c60fa;
}

.caret {
    margin-top: 22px;
    float: right;
    margin-right: 4px;
}

span.btnLabel {
    max-width: 90%;
    display: inline-block;
    overflow: hidden;
}


.dropdown {
    width:100%;
}

.dropdown-toggle {
    border-radius:0 !important;
    width:100% !important;
    font-weight:600!important;
    font-size:16px;
    text-align:left;
    background-color:#fff;
}

.counter {
    display: inline;
    margin: 0;
    text-align:left !important;
}

.statsBlock .counter .count {
    display: inline-block !important;
    border: 0 !important;
    color: #287B96 !important;
    text-decoration:underline;
    font-weight: 600 !important;
    font-size: 16px !important;
    font-family: 'Roboto Condensed', sans-serif;
    -webkit-border-radius: 0px!important;
    border-radius: 0 !important;
    line-height: 20px !important;
    padding: 0 7px;
    cursor: pointer;
    height:initial !important;
    background-color: none;
}

.magic {
    width: 50px;
    height: 50px;
    position: absolute;
    top: 0;
    right: 0;
    display: none;
}


a {
text-decoration: none;
}

#report {
    margin-top:20px;
}

#report .row {
    padding:5px;
}



.dropdownWrapper {
    padding:30px 0;
    background-color: #DAEFF5;
    display:none;
}


.control-label{
    font-family: 'Open Sans', Sans-Serif;
    text-transform: none;
    font-weight:600;
    color:#000;
    font-size:14px !important;
    position:relative;
    line-height: 50px;
    padding: 0;
    height: 50px;
}

.label {
    display: inline-block;
    margin-top: 3px !important;
    font-weight: bold;
    margin-bottom: 0 !important;
}

#group {
    font-family: 'Open Sans', Sans-Serif;
    font-size: 16px;
    font-weight:600;
    text-transform:uppercase !important;
    padding: 2px;
    background-color:#fafafa;
}

.statsBlock {
    font-family: 'Open Sans', Sans-Serif;
    display:inline-block;
    float:left;
    width:0;
    height:20px;
    font-weight:600;
    background:#3FE0FF;
    background:rgb(62, 200, 226);
    opacity:0.9;
    font-size:16px;
    text-transform:uppercase;
    margin-right:5px;
}

.right {
    float: right;
    text-transform: uppercase;
    font-size: 16px;
    font-weight: 400;
}

#report .leftSide {
    padding-right:10px;
}

#report .rightSide {
    padding-left:10px;
}

.statTitle {
    text-transform: uppercase;
    font-size: 40px;
    font-weight: 700;
    padding-left: 20px;
    color: #B1AFAF;
    font-family: 'open sans condensed';
}

/*.statTitle.text-right {
    padding-right:20px !important;
}*/

.statTitle.rightSide {
   line-height: 65px;
   font-style: italic;
   font-family: 'open sans';
}

.statTitle.leftSide {
    font-size: 46px !important;
    font-weight: 400;
    color: #000;
}

#spacerow{
    border:0; 
    padding:0;
    height:10px;
}

.span.right {
    color:#000000 !important;
}


.dropdown-menu {
    text-transform: uppercase;
    min-width:100%;
}

.dropdown-menu > li > a {
    padding: 9px 10px;
}

.resultsWrapper {
    position:relative;
}

/*By Interest Report*/

#ByInterest #stripesBackground {
    background: url(/Content/images/stripes2.png);
    left: 50%;
    top: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    margin:4px;
}



/*================By trait report=================*/

#traitReportPage #stripesBackground {
    background: url(/Content/images/stripes2.png);
    left: 50%;
    top: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    margin:4px;
}


#traitReportPage .leftSide {
    position:relative;
}

#traitReportPage .leftSide div {
    position: absolute;
    top: 0;
    right: 0;
    text-transform: uppercase;
    color: #E1DEDE;
    font-size:46px;
    line-height: 46px;
    font-weight: 700;
    font-family: 'open sans condensed';
    margin-right:10px;
}

#traitReportPage .leftSide div.odd {
    font-size: 34px;
}


/*#traitReportPage .statTitle.rightSide {
    font-size: 40px !important;
    font-weight: 400;
    color: #000;
}

#traitReportPage.statTitle.leftSide {
    position:relative;
}*/


#traitReportPage .right {
    float: right;
    text-transform: uppercase;
    font-size: 15px;
    font-weight: 400;
    position: absolute;
    left: 20px;
    color: #fff;
    z-index: 100;
}

#traitReportPage .statsBlock {
    margin-bottom:3px;
    max-width:350px;
}

#traitReportPage .left {
    float: left;
    text-transform: uppercase;
    font-size: 14px;
    width: 100px;
    font-weight: 400;
    left: 20px;
    color: #fff;
    background: rgb(62, 200, 226);
    opacity: 0.8;
    z-index: 100;
    padding-left:7px;
}


/**============Careers Report===============***/

#careersReport #stripesBackground {
    background: url(/Content/images/stripes2.png);
    left: 50%;
    top: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    margin:4px;
}

#careersReport .statTitle {
    text-align: center;
    margin: 30px 0 20px;
    font-size: 34px !important;
    font-weight: 400;
    color: #000;
}

.statWrapper {
    width: 300px;
    max-width: 300px;
}

#careersReport .statsBlock {
    max-width: 300px;
}


/***================Student List================***/
#studentListPage h1 {
    margin-top:15px;
}

.subheading {
    font-size: 18px;
    font-weight: 300;
    text-align: center;
    margin: 15px 0 0;
}

.studentNames {
    padding:30px;
}


.studentName {
    background-color: #DAEFF5;
    margin: 10px;
    cursor:pointer;
    position:relative;
}

.studentName:hover, .studentName:focus, .studentName:active {
    -moz-box-shadow: 0px 0px 2px 2px #B9D3D5;
-webkit-box-shadow: 0px 0px 2px 2px #B9D3D5;
box-shadow: 0px 0px 2px 2px #B9D3D5;
}

.studentName a {
    display:block;
    padding:15px;
    font-size:18px;
    padding-right:50px;
    color:#000;
    text-decoration:none;
}

.arrowRight {
    right:10px;
}




/*==============MIN WIDTH=================*/
@media screen and (min-width:768px) {
    .reportHeader {
    font-family: 'Open Sans', Sans-Serif;
    color: #000000;
    position: relative;
    padding: 2px;
    width: 100%;
    }

    h2 {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: uppercase;
    font-weight: 300;
    color: #000000;
    font-size: 26px;
    line-height: 27px;
    height: 31px;
    padding: 2em;
    letter-spacing: -2px;
    }
        
    /*p {
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 300;
    color: #000000;
    font-size:24px !important;
    margin-bottom:3em;
    margin-top: 35px;
    }*/


    .graphicon {
    height: 75px;
    }

    .reportDiv {
    font-family: 'Open Sans', Sans-Serif;
    /* text-transform: uppercase;*/
    font-weight: 600;
    color: #000000;
    font-size: 20px !important;
    }

    #control-label.special {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: none;
    font-weight: 600 !important;
    color: #000000;
    font-size: 14px !important;
    float: right !important;
    }
}



@media only screen and (max-width:1200px) {
    span.btnLabel {
        font-size: 14px;
    }

    .dropdown-menu > li > a {
        font-size: 14px;
    }

    .reportDiv {
        padding:15px;
    }

    .reportDiv p {
        font-size:17px;
    }

    #traitReportPage .statsBlock {
        margin-bottom:3px;
        max-width:300px;
    }
}


@media only screen and (max-width: 992px) {

    .pageContainer .container {
        width:95%;
    }

    .pageContainer h1 {
        font-size:30px;
    }

    .reportDiv h3 {
        text-align:left;
    }

    .reportDiv p {
        font-size:16px;
        margin-bottom:0;
    }

    .dropdown {
        margin-left:5px;
    }

    .dropdownWrapper .noResultsError {
        text-transform:none;
    }

    #traitReportPage .statsBlock {
       max-width:200px;
    }

    #traitReportPage .left {
        width:92px;
        font-size:13px;
        padding-left: 5px;
        line-height:20px;
    }

    .noResultsError p, .noResultsError a {
        font-size: 20px;
    }
}

@media only screen and (max-width:900px) {
    .reportDiv {
        min-height:180px;
    }
}

@media only screen and (max-width:767px) {
    .dropdownWrapper {
        padding: 0 0 15px;
    }
    .reportDiv {
        min-height:initial;
    }

    .pageContainer h1 {
        font-size:28px;
        margin: 0 0 20px;
    }

    .statTitle {
        font-size: 34px;
    }

    .statTitle.leftSide {
        font-size: 40px !important;
    }

    .statTitle.rightSide {
        line-height: 60px;
    }

    .pageContainer .container {
        width:96%;
    }

    #traitReportPage .leftSide div {
        font-size:34px;
    }

    #traitReportPage .leftSide div.odd {
        font-size: 23px;
    }

    .control-label {
        height:40px;
        line-height:40px;
        margin-top:10px;
    }

    .right {
        text-align:left;
        float:none;
    }

    #careersReport .right {
        float:left;
    }

    #careersReport #report .leftSide, #careersReport #report .rightSide {
        padding-left:0;
    }

    #careersReport #stripesBackground {
        left:0;
    }

    #careersReport .statTitle {
        text-align: center;
        margin: 20px 0 10px;
        font-size: 30px !important;
        font-weight: 400;
        color: #000;
    }

    .dropdownWrapper .noResultsError {
        font-size: 20px;
        display: none;
        margin: 0;
        text-align: left;
        padding: 20px 15px;
        font-family: 'open sans' !important;
        text-transform: none;
    }

    .dropdown {
        margin-left:0;
    }
    
}


