@media screen and (max-width: 500px) {
    .pageContainer .container {
        padding:10px 5px 20px;
        width:100%;
    }

    .pageContainer h1 {
        font-size:20px;
        margin: 0 0 10px;
    }

    .right {
        float:left;
        font-size: 14px !important;
    }

    .statTitle {
        font-size: 24px;
    }

    .statTitle.rightSide {
        line-height: 34px;
    }

    .statTitle.leftSide {
        font-size: 24px !important;
    }

    .leftSide {
        padding-left:0;
    }

    .rightSide {
        padding-right:0;
    }

    #report {
     margin-top: 10px;
    }

    span.btnLabel {
        font-size: 12px;
    }

    .dropdown-menu > li > a {
        font-size: 12px;
    }

   #traitReportPage .leftSide div {
        margin-right:5px;
        font-size:24px;
    }

   #traitReportPage .leftSide div.odd {
        font-size: 20px;
    }

    #careersReport .statTitle {
        font-size: 25px !important;
        padding: 0;
    }

    .noResultsError p, .noResultsError a {
        font-size: 14px;
    }

}


