.pageContainer {
    background-attachment:inherit;
    background-image:url("../../images/hex/hex_bg14.jpg");
    position:relative;
    padding-bottom:0;
}

.pageContainer .container {
    background-color:#fff;
    height:auto;
    padding:20px 20px 50px;
}

/*p {
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 600;
    font-size: 20px;
    padding: 0;
}*/

.pageContainer h1 {
    font-size: 34px;
    font-family: 'open sans condensed';
    margin: 0 0 30px;
    font-weight: 600;
    display: block;
    vertical-align: middle;
    text-align:center;
}

.pageContainer h2 {
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 600;
    font-size: 16px;
    margin-top: 10px;
    margin-bottom: 5px;

}

.pageContainer h3 {
    font-family: 'Open Sans', Sans-Serif;
    font-weight: 600;
    font-size: 20px;
    margin-top: 5px;
    margin-bottom: 5px;
}

.reportWrapper.special h3 {
    height:25px;
    margin-top:36px;
}

.dropdownWrapper .noResultsError {
    padding: 0;
    font-size: 20px;
    display:none;
}

.noResultsError p, .noResultsError a {
    text-transform: none;
    font-size: 22px;
    margin-bottom:0;
}


/*==================Index Page=================*/
.graphicon {
    height: 76px;
    margin-right: 20px;
    position: absolute;
    top: 50%;
    margin-top: -37px;
}

.graphicon.special {
    height:100px;
}

.reportWrapper {
    display:table-cell;
    vertical-align:middle;
    padding-left:90px;
    /*position:absolute;
    top:50%;*/

}

.reportDiv {
    padding: 30px;
    border: 1px solid #D2FAFB;
    margin: 0 0 30px;
    cursor:pointer;
    /*box-shadow: rgba(0,0,0,0.2) 10px 10px 20px ;*/
    position:relative;
}

.reportDiv:hover {
    /*-moz-box-shadow: 0 0 5px #888;
    -webkit-box-shadow: 0 0 5px#888;
    box-shadow: 0 0 5px #888;*/
    background-color:#E5F5FA;
}

.reportDiv h3 {
    text-align: left;
}

.reportDiv p {
    font-weight:300;
    font-size:19px;
    margin:10px 0 0;
}

.iconBox {
    float: left;
    height: 120px;
    line-height: 120px;
}

.wrapper {
    margin-bottom: 0;
    padding: 2em;
}
    
.wrapper.style1 {
    background:#ffffff;
    padding-bottom:2em;    
}
.wrapper.style2 {
    background:#fff;
    padding:0; 
}  


#canvas_container {
    position: fixed;
    top: 0;
    left: 0;
    height: 755px;
    width: 610px;
}

/*svg[Attributes Style] {
    height: 755px;
    width: 610px;
}*/

