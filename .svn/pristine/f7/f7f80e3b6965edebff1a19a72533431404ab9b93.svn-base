@{
    ViewBag.Title = "Report Card";
}

<!--========================Student Report Page========================================-->

<link href="~/Content/css/Skilltest/locked.css" rel="stylesheet" />
<link href="~/Content/css/Assessment/assessment.css?v=@ViewBag.AppVersion" rel="Stylesheet" />
<link href="~/Content/css/Analytics/reportcard.css?v=@ViewBag.AppVersion" rel="Stylesheet" />
<script src="~/Scripts/Thesaurus/testattributes.js?v=@ViewBag.AppVersion"></script>
<script src="~/Scripts/Thesaurus/ctProfessions.js?v=@ViewBag.AppVersion"></script>
<script src="~/Scripts/Thesaurus/ctProfessionsCodes.js?v=@ViewBag.AppVersion"></script>
<script src="~/Scripts/Thesaurus/careermajorsmapping.js?v=@ViewBag.AppVersion"></script>
<script src="~/Scripts/Thesaurus/bachelormajors.js?v=@ViewBag.AppVersion"></script>

<div class="pageContainer whiteBgPage blueBackground lockedMode" id="studentReportPage">
    <div class="container">
        <div class="reportWrapper col-lg-9 col-md-9 col-sm-9 col-xs-12">
            <div class="row socialIconsHolder">
                <div class="fb-like" data-href="@Request.Url.AbsoluteUri" data-layout="button_count" data-action="like" data-show-faces="false" data-share="true"></div>
                <script type="IN/Share" data-counter="right">
                </script>
                <div class="g-plus" data-action="share" data-annotation="bubble"></div>
                <div class="inline-div"><a href="https://twitter.com/share" class="twitter-share-button">Tweet</a></div>
            </div>
            <div class="reportWrap">
                @*<div class="reportCard">Report Card</div>*@
                <div class="reportLink pull-right printLink noPrint" onclick="window.print()">
                    <img src="../../Content/images/print3.png" alt="" />
                    Print this page
                </div>
                <h1>Your report card</h1>

                <div class="separator"></div>

                <!--========================Traits Table========================================-->
                <div class="reportDiv">
                    <h2><span>Your Personality Traits</span><i class="fa fa-angle-down arrowDown"></i><i class="fa fa-angle-up arrowUp"></i></h2>
                    <div class="personalityReport reportSection" id="personalityReport">
                        <div class="row">
                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell cellInversed col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>

                            <div class="cell col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            </div>
                        </div>
                    </div>
                    <div class="noResultsError">
                        You need to take the career test before viewing results
                    </div>
                    <div class="text-right row extraMargin">
                        <a class="reportLink" href="/SkillTest">Go to Career Test</a>
                    </div>
                </div>

                <div class="separator"></div>
                <!--========================About you========================================-->
                <div class="reportDiv">
                    <h2><span>About you</span><i class="fa fa-angle-down arrowDown"></i><i class="fa fa-angle-up arrowUp"></i></h2>
                    <div class="aboutYouReport reportSection" id="aboutYouReport">
                    </div>
                    <div class="noResultsError">
                        You need to take the career test before viewing results
                    </div>
                    <div class="text-right row extraMargin"><a href="/CareerTest" class="reportLink">Go to Career Test</a></div>
                </div>

                <div class="separator"></div>

                <!--========================Careers matching your personality ========================================-->
                <div class="reportDiv">
                    <h2><span>Your matching careers</span><i class="fa fa-angle-down arrowDown"></i><i class="fa fa-angle-up arrowUp"></i></h2>
                    <div class="careersReport reportSection" id="careersReport">
                    </div>

                    <div class="noResultsError">
                        You need to take the career test before viewing results
                    </div>
                    @*<div class="text-right row extraMargin">
                            <a href="/CareerTest" class="reportLink">Go to Career Test</a>
                        </div>*@
                </div>

            </div>
        </div>
        <div class="rightColumn col-lg-3 col-md-3 col-sm-3 col-xs-12">
            <div class="rightSide">
                <a class="sideNav" href="/Careers/ByPersonality">Your Career Results<i class="fa fa-angle-right"></i></a>
                <a class="sideNav activeTab" href="/TakeTest/TestReport">Your Report Card</a>
                <a class="sideNav" href="/TakeTest/SchoolResults">Your School results<i class="fa fa-angle-right"></i></a>
            </div>
            <div class="ad">
                <h6 class="advertisement">Advertisement</h6>
                <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                <!-- CareerThesaurus -->
                <ins class="adsbygoogle"
                     style="display:block"
                     data-ad-client="ca-pub-3486420009474810"
                     data-ad-slot="6576327288"
                     data-ad-format="auto"></ins>
                <script>
                    (adsbygoogle = window.adsbygoogle || []).push({});
                </script>
            </div>
        </div>
    </div>
</div>

<script>
    var isiPad = "@ViewBag.IsiPad".toLowerCase() == 'true' ? true : false;
    var matchedCareers = [];
    var cellIndex = 0;
    var emptyCells = 0;
    function document_ready() {

        $(window).resize(function () {
            if ($(window).width() < 768) {
                $('.rightSide').insertBefore('.reportWrapper');
            }
        });

        var showDivs = false;
        var showDivs2 = false;

        for (var property in dimensions) {
            if (dimensions.hasOwnProperty(property)) {
                dimensions[property] = getCookie(property) || '';
                if (getCookie(property) == '' || getCookie(property) == null) {
                    emptyCells++;
                }
            }
        }

        if (emptyCells != 0) {
            $('.reportSection').hide();
            $('.noResultsError').show();
        } else {
            $('.reportSection').show();
            $('.noResultsError').hide();
        }

        for (var i = 0; i < dimensiondefinitions.length; i++) {
            $('.cell').eq(i).html(dimensiondefinitions[i].values.filter(function (x) { return x.value == dimensions[dimensiondefinitions[i].name]; })[0].caption);
        }


        if ($('.cell').eq(0).find('.firstLine').text() == "Extrovert") {
            $('#aboutYouReport').append('<div class="traitsDescription notViewed"><h3>You are an extrovert</h3><p>You thrive on collaboration and love to exchange ideas with your colleagues. A noisy office area or crowded conference room is the best place for you to get creative and find solutions to challenges. Your colleagues love the fact that you are able to approach just about anyone, so you may find yourself representing your team at meetings or off-site events where social skills are just as important as business skills. You\'ll have no problem succeeding at these events, as networking with others is a breeze for an extravert like you. Working with others gives you the energy you need to produce top results.</p><div class="text-right"><a href="/personality/extrovert" class="blueStripesBtn">Show Extrovert Careers <i class="fa fa-angle-right pull-right arrowRight"></i></a></div></div>');
        } else if ($('.cell').eq(0).find('.firstLine').text() == "Introvert") {
            $('#aboutYouReport').append('<div class="traitsDescription notViewed"><h3>You are an introvert</h3><p>You excel at working independently and need little guidance when completing projects. Managers love that you are able to take a set of project guidelines and hit the ground running with little input from other team members. Like other introverts, you prefer working in a private space to spending time in a conference room or other group environment. Working alone gives you the energy to power through tough assignments and get things done quickly, making you a valuable asset to any employer. E-mail is your preferred method of communication, as it lets you think about what you are going to say before you send a message.</p><div class="text-right"><a href="/personality/introvert" class="blueStripesBtn">Show Introvert Careers <i class="fa fa-angle-right pull-right arrowRight"></i></a></div></div>');
        }


        if ($('.cell').eq(1).find('.firstLine').text() == "Stationary") {
            $('#aboutYouReport').append(' <h3>You prefer to remain stationary</h3><p>Having an opportunity to move around during the workday isn\'t something that is important to you. Instead, you love to spend time sitting at your desk and powering through everything from routine tasks to unexpected challenges. As someone who prefers a stationary career, a desk job will actually cause you less stress than a job that has you moving constantly. Spending a lot of time at your desk also gives you the time you need to think about your work and make good decisions. Managers appreciate that you are usually available for impromptu meetings or telephone calls.</p><div class="text-right"><a href="/personality/Stationary" class="blueStripesBtn">Show stationary careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(1).find('.firstLine').text() == "Mobile") {
            $('#aboutYouReport').append('<h3>You prefer to be on the move</h3><p>You prefer to be on the go most of the time, whether you are attending a meeting across town or escorting clients on a tour of your city. Jobs that require a lot of desk time do not appeal to your sense of adventure. You excel in jobs that require a great deal of flexibility, as you are more than willing to drop what you are doing and start a new task in a new location. You may also enjoy jobs that require athletic prowess or physical stamina, as they make use of your natural tendency to move around while making decisions.</p><div class="text-right"><a href="/personality/Mobile" class="blueStripesBtn">Show mobile careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }


        if ($('.cell').eq(2).find('.firstLine').text() == "Process") {
            $('#aboutYouReport').append('<h3>You are process oriented</h3><p>How you perform your job tasks is more important to you than the tasks themselves. This is a good thing, as focusing on these processes helps you learn and grow as a person. You place a high level of importance on learning new things and understanding exactly why things work as they do. This emphasis on learning helps you develop new skills that will increase your worth as an employee and help you qualify for special projects or promotions. You do best when performing clearly-defined tasks and participating in work activities that have a clear agenda.</p><div class="text-right"><a href="/personality/process" class="blueStripesBtn">Show process oriented careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(2).find('.firstLine').text() == "Result") {
            $('#aboutYouReport').append('<h3>You are result oriented</h3><p>Results-oriented people like yourself thrive in challenging environments where the end result is more important than the process used to achieve a task. When you are assigned a new project, you picture the end result in your mind and work to achieve that result as quickly as possible. Managers rely on you to make quick decisions that will help the entire team get the best result possible, and colleagues value your ability to get things done. Your results-oriented approach motivates other people to act quickly, so you may excel as a team leader, supervisor, or manager.</p><div class="text-right"><a href="/personality/result" class="blueStripesBtn">Show result oriented careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }


        if ($('.cell').eq(3).find('.firstLine').text() == "Focus") {
            $('#aboutYouReport').append('<h3>You prefer to stay focused</h3><p>You excel in careers that require a great deal of concentration, especially when you are able to work in a quiet environment that does not have a lot of distractions. Colleagues and supervisors rely on you to buckle down and focus on the details, as your personality makes it easy for you to get through marathon work sessions that would get the best of other employees. Your ideal work environment is one that has very little foot traffic, as you do not like to be disturbed by visitors or people who have to walk past your work area to get to other parts of the office.</p><div class="text-right"><a href="/personality/focused" class="blueStripesBtn">Show focused careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(3).find('.firstLine').text() == "Relaxed") {
            $('#aboutYouReport').append('<h3>You prefer relaxed environment</h3><p>While some people avoid distractions during work hours, you thrive on them. You love when colleagues stop by your office to discuss upcoming projects or ask for your input in solving a work-related problem. Instead of being annoyed by ringing telephones and clicking computer keys, you find it almost impossible to work without some sort of background noise. It is actually easier for you to meet deadlines when you have a few distractions, as you prefer breaking work sessions into small chunks of time rather than working straight through without any interruptions.</p><div class="text-right"><a href="/personality/relaxed" class="blueStripesBtn">Show relaxed careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }

        if ($('.cell').eq(4).find('.firstLine').text() == "Fact") {
            $('#aboutYouReport').append('<h3>You are a fact seeker</h3><p>When it\'s time for you to make a decision, you\'re the type to "believe it when you see it." Rather than relying on your imagination, you rely on concrete facts and statistics. Supervisors love your rational approach to problem-solving, which helps you identify the potential pitfalls of business deals before your colleagues do. Rather than make on-the-spot decisions, you prefer to gather as much information as possible. This approach to your work makes you a reliable team member who others count on when it\'s time to make tough decisions. You thrive in careers that require excellent analytical skills and strong problem-solving abilities.</p><div class="text-right"><a href="/personality/facts" class="blueStripesBtn">Show factual careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(4).find('.firstLine').text() == "Imagination") {
            $('#aboutYouReport').append('<h3>You are an imaginator</h3><p>Your approach to problem-solving allows you to thrive in careers that require creativity and imagination. Rather than filling your mind with statistics and facts, you prefer to rely on your own imagination to come up with solutions and innovative ideas for new products or services. While some people prefer a step-by-step approach to problem-solving, you prefer to make imaginative leaps from one point to another. Your colleagues benefit from this approach, as it helps you come to conclusions that step-by-step thinkers would not come up with on their own. Managers especially appreciate your ability to come up with unique ways to solve problems.</p><div class="text-right"><a href="/personality/imagination" class="blueStripesBtn">Show imaginative careers <i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
            $('.cell').eq(4).find('.firstLine').addClass('inversed');
            $('.cell').eq(4).find('.secondLine').addClass('inversed');
        }

        if ($('.cell').eq(5).find('.firstLine').text() == "Rational") {
            $('#aboutYouReport').append('<h3>You are rational</h3><p>As a rational thinker, you prefer to rely on concrete information rather than emotions or instinct. Colleagues value your logical approach to problem-solving, especially when critical decisions need to be made. Managers rely on your use of logic to solve complex problems, so you are a valued team member in any environment. Although you prefer to use logic when making decisions, you still have the ability to "go with your gut" when necessary. The ability to use logic even under the toughest of circumstances will help you succeed in careers involving technology and scientific processes.</p><div class="text-right"><a href="/personality/rational" class="blueStripesBtn">Show imaginative careers <i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(5).find('.firstLine').text() == "Emotional") {
            $('#aboutYouReport').append('<h3><h3>You are emotional</h3><p>While some people prefer to deal with concrete information when making decisions, you rely on your instincts to guide you. Emotions and feelings play an important role in your decision-making process, so you are used to dealing with some level of uncertainty on the job. Your ability to roll with the punches helps you succeed in the toughest work environments, and colleagues rely on your gut feelings to guide them in making their own decisions. You have a high level of emotional intelligence, so you excel in careers that require the ability to read other people and use the information you gather to make decisions.</p><div class="text-right"><a href="/personality/emotional" class="blueStripesBtn">Show imaginative careers <i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }


        if ($('.cell').eq(6).find('.firstLine').text() == "Remotely") {
            $('#aboutYouReport').append('<h3>You can work remotely</h3><p>When it comes to interacting with colleagues, supervisors, and clients, you prefer e-mail and telephone conversations to actual face time. You thrive when you are able to work off-site, especially if you are able to make your own schedule. Freelance jobs and non-traditional jobs appeal to you because they give you the freedom to set your own hours and work when you are at your peak performance. Clients and managers don\'t mind giving you a lot of flexibility, as you achieve the best results when you are allowed to set your own pace and decide when and where you will work.</p><div class="text-right"><a href="/personality/remote" class="blueStripesBtn">Show remote careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
            $('.cell').eq(6).find('.firstLine').addClass('inversed');
            $('.cell').eq(6).find('.secondLine').addClass('inversed');
        } else if ($('.cell').eq(6).find('.firstLine').text() == "Presence") {
            $('#aboutYouReport').append('<h3>Your presence is required</h3><p>You thrive in careers that require frequent contact with your clients and colleagues. Although you have the ability to work independently, you do not value independence above face time with those who need you most. Your supervisors love the fact that they never have to wonder where you are, and colleagues appreciate that you make yourself available to assist them with projects and listen to their ideas. You are perfectly content to spend most of your time in the office, so a traditional 9-to-5 job would appeal to you more than a job that required telecommuting.</p><div class="text-right"><a href="/personality/present" class="blueStripesBtn">Show in-person careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }


        if ($('.cell').eq(7).find('.firstLine').text() == "Discovery") {
            $('#aboutYouReport').append('<h3>You constantly discover</h3><p>You are somewhat of a perfectionist when it comes to your work methods. Rather than use the same process for every task, you love to explore new ways of doing things. Increasing efficiency and improving the quality of your work are always at the front of your mind. You excel in careers that give you plenty of opportunities for finding new ways to solve problems or perfect processes. What works for others may not work for you, so colleagues and supervisors think of you as an innovative person who will settle for nothing less than the best when it comes to getting things done.</p><div class="text-right"><a href="/personality/discovery" class="blueStripesBtn">Show discovery careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
            $('.cell').eq(7).find('.firstLine').addClass('inversed');
            $('.cell').eq(7).find('.secondLine').addClass('inversed');
        } else if ($('.cell').eq(7).find('.firstLine').text() == "Routine") {
            $('#aboutYouReport').append('<h3>You prefer an established process</h3><p>While some people are never content with their work processes, you are happy to stick with the status quo. You are one of those people who find a way to automate every task possible, so your colleagues think of you as someone who is efficient and committed to maintaining a high level of productivity. Computers, smart phones, and other gadgets are your most trusted tools, as you use them to automate routine tasks that require very little in the way of concentration. Because you are content with routine, you do not get bored as easily as people who enjoy finding new ways to complete tasks.</p><div class="text-right"><a href="/personality/routine" class="blueStripesBtn">Show procedural careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
            $('.cell').eq(7).find('.firstLine').addClass('inversed');
            $('.cell').eq(7).find('.secondLine').addClass('inversed');
        }

        if ($('.cell').eq(8).find('.firstLine').text() == "Variable") {
            $('#aboutYouReport').append('<h3>You prefer variable income</h3><p>When it comes to your compensation, you are willing to take a risk if it means that you will make more money than you would if you accepted a fixed salary. You jump at the chance to take jobs that pay a base salary plus commissions, bonuses, or incentive payments. Because your performance affects how much money you make, you strive to do your best at every task. Colleagues and managers alike think of you as a go-getter who refuses to accept anything less than a first-place finish. This drive helps you earn as much income as possible while enjoying the personal satisfaction of achieving your goals.</p><div class="text-right"><a href="/personality/variable" class="blueStripesBtn">Show variable income careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        } else if ($('.cell').eq(8).find('.firstLine').text() == "Fixed") {
            $('#aboutYouReport').append('<h3>You prefer fixed income</h3><p>Because you are naturally averse to taking financial risks, you prefer to receive a fixed salary or hourly wage. You shy away from positions that involve incentive plans or commissions, as they do not offer you the financial stability you desire. Receiving a fixed income helps you budget for expenses and determine how much money you need to save to meet your financial goals. If you have family members who rely on you for their basic needs, choosing a guaranteed salary over a base salary plus performance-based commissions helps you meet your obligations and put away money for the future.</p><div class="text-right"><a href="/personality/fixed" class="blueStripesBtn">Show fixed income careers<i class="fa fa-angle-right pull-right arrowRight"></i></a></div>');
        }

        var answeredTraitsCount = $('.traitsDescription p').length;

        //if (greating.toLowerCase() != 'you') {
        //    $('.reportLink').closest('div').hide();
        //    $('.blueStripesBtn').hide();
        //    $('.printLink').show();
        //}

        $('body').scrollTop(0);
        $('html').scrollTop(0);
        window.scrollTo(0, 0);
        $(document).scrollTop(0);
        $(window).scrollTop(0);

        $('h2').click(function () {
            var sectionName = $(this).next('.reportSection').attr('id');
            //show
            if (!($(this).next('.reportSection').is(':visible'))) {
                $(this).next('.reportSection').velocity('fadeIn', { duration: 300 });
                $(this).find('.arrowDown').hide();
                $(this).find('.arrowUp').show();
                setCookie('expandSection' + sectionName, 'yes', 365);
                setTimeout(function () {
                    animateResults();
                }, 300);
            }
            else { //hide
                $(this).next('.reportSection').velocity('fadeOut', { duration: 300 });
                $(this).find('.arrowDown').show();
                $(this).find('.arrowUp').hide();
                setCookie('expandSection' + sectionName, 'no', 365);
                setTimeout(function () {
                    animateResults();
                }, 300);
            }
        });


        $('.reportSection').each(function (i, element) {
            var sectionName = $(element).attr('id');
            if (getCookie('expandSection' + sectionName) == 'no') {
                $(element).hide();
                $(element).closest('.reportDiv').find('.arrowDown').show();
                $(element).closest('.reportDiv').find('.arrowUp').hide();

            }
            else {
                $(element).show();
                $(element).closest('.reportDiv').find('.arrowDown').hide();
                $(element).closest('.reportDiv').find('.arrowUp').show();
            }
        });

        //center demo heading
        if ($(window).width() > 750) {
            $('.col1').css({ 'margin-top': ($('.col1').innerHeight() / 2 * -1) + 'px' });
        }
        $(window).resize(function () {
            if ($(window).width() > 750) {
                $('.col1').css({ 'margin-top': ($('.col1').height() / 2 * -1) + 'px', 'position': 'absolute' });
            }
            else {
                $('.col1').css({ 'margin-top': '0', 'position': 'static' });
            }
        })

        animateResults();

        // animateTraits();

        //animate trait descriptions
        function animateTraits() {
            var loaded = [];
            if ($(window).width() > 767) {
                if (isiPad == false) {
                    for (var k = 0; k < answeredTraitsCount; k++) {
                        if (loaded[k] != "1") {
                            var bottom_of_object = $(".traitsDescription").eq(k).position().top;// + $(".traitsDescription").eq(k).outerHeight();
                            var bottom_of_window = $(window).scrollTop() + $(window).height();
                            /* If the object is completely visible in the window, fade it it */
                            if (bottom_of_window > bottom_of_object) {
                                $(".traitsDescription").eq(k).velocity("transition.slideUpIn").velocity({ opacity: 1 }, 0);
                                loaded[k] = "1";
                            }
                        }
                    }
                    $(window).scroll(function () {
                        for (var k = 0; k < answeredTraitsCount; k++) {
                            if (loaded[k] != "1") {
                                var bottom_of_object = $(".traitsDescription").eq(k).position().top;// + $(".traitsDescription").eq(k).outerHeight();
                                var bottom_of_window = $(window).scrollTop() + $(window).height();
                                /* If the object is completely visible in the window, fade it it */
                                if (bottom_of_window > bottom_of_object) {
                                    $(".traitsDescription").eq(k).velocity("transition.slideUpIn").velocity({ opacity: 1 }, 0);
                                    loaded[k] = "1";
                                }
                            }
                        }
                    });
                }
                else { //ipad == true and over 767px
                    $('.traitsDescription').css('opacity', '1');
                }
            }
            else { //less than 767
                $('.traitsDescription').css('opacity', '1');
            }

            if (isiPad == true) { //no animation for ipad
                $('.traitsDescription').css('opacity', '1');
            }
        }

        function animateResults() {
            if ($(window).width() > 767) {
                $('.traitsDescription.notViewed').each(function (i, elem) {
                    var top_of_object = $(elem).position().top;
                    var bottom_of_window = $(window).scrollTop() + $(window).height();
                    /* If the object is completely visible in the window, fade it it */
                    if (bottom_of_window > top_of_object) {
                        // $("this").velocity("transition.slideUpIn").velocity({ opacity: 1 }, 0);
                        $(elem).removeClass('notViewed').addClass('viewed');
                    }
                })

                $(window).scroll(function () {
                    $('.traitsDescription.notViewed').each(function (i, elem) {
                        var top_of_object = $(elem).position().top;
                        var bottom_of_window = $(window).scrollTop() + $(window).height();
                        /* If the object is completely visible in the window, fade it it */
                        if (bottom_of_window > top_of_object) {
                            // $(elem).velocity("transition.slideUpIn").velocity({ opacity: 1 }, 0);
                            $(elem).removeClass('notViewed').addClass('viewed');
                        }
                    })
                })
                if (isiPad == true) {
                    $('.traitsDescription.notViewed').addClass('viewed');
                }
            }
            else { //less than 767
                $('.traitsDescription.notViewed').addClass('viewed');
            }
        }


        //Show Careers matching personality
        //  if (answeredTraitsCount == 9) {
        matchedCareers = matchResults();
        matchedCareers.sort(sortByEdu);
        var currentEduLevel = -1;
        for (var i = 0; i < matchedCareers.length; i++) {
            var item = matchedCareers[i];
