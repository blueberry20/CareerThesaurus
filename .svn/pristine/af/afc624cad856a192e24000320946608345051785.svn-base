
html, body {
    height: 100%;   
}

* {
    font-family: 'Open Sans', sans-serif;
}

h1, h2, h3, h4, h5, h6 {
    font-family: 'Open Sans', Sans-Serif;
    text-transform: uppercase;
    font-weight:400;
    color:#000;   
}

h1 {
    font-size:30px;
}

h2 {
    font-size:26px;    
}

h3 {
    font-size:20px;   
}

h4 {
    font-size:16px;   
}


/* leave space for nav menu on all pages*/
.pageContainer {
    padding: 50px 0 0px;
    background-repeat: repeat;
    background-color: #fdfdfd;
    background-color: #F7FBFB;
    /*background-image: url('/Content/images/hex/subtlehex.jpg');*/
}

.pageContainer .container {
    min-height:600px;
}


.section {
    border: 0;
    outline: 0;
    font-size: 100%;
    vertical-align: baseline;
    background: transparent;
    margin: 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
    overflow: hidden;
    width: 100%;
    z-index: 2;
    padding: 100px 0;
    background-color: transparent;
}

.section h2 {
    padding-bottom:50px;   
}

/*override bootstrap container padding*/
.container {
    padding:0;   
}

.row {
    margin:0;   
}

.bold {
   font-weight:600; 
}
    
.btn {
    width: 150px;   
    text-transform: uppercase;
    height:50px;
    min-width:150px;
    line-height:50px;
    padding:0;
}

button, .btn {
    outline:0;
}

.shortBtn {
    min-width: initial;
    width: initial;
    padding: 0 15px;
}

.btn-success {
    background-color: #38D5A3;
    border:0;
}

.btn-success:hover, .btn-success:focus, .btn-success:active {
    background-color: #2CAD84;
}

.btn-info {
    background-color:rgb(23, 184, 214);
    border:0;
}

.btn-info:hover, .btn-infor:focus, .btn-info:active {
    background-color:rgb(18, 157, 183);
}

.blueBorderBtn {
    width: 100%;
    outline: 0;
    border: 2px solid #27AEC0;
    font-weight: 600;
    text-transform: uppercase;
    padding: 0 2px 0 8px;
    line-height: 40px;
    display: block;
    text-align: center;
    background-color: #fff;
    text-decoration: none;
    color: #000;
}

.blueBorderBtn:hover, .blueBorderBtn:active, .blueBorderBtn:focus {
    background-color: rgba(23, 184, 214, 1);
    text-decoration: none;
    color: #000;
}

.blueStripesBtn {
    background-color: transparent;
    border: none;
    height: 50px;
    padding: 5px 10px;
    text-transform: uppercase;
    font-weight: 600;
    font-size: 17px;
    padding-right: 30px;
    background-image: url(/Content/images/stripespattern8.png);
    margin: 10px 0;
    position:relative;
}

.blueStripesBtn:hover, .blueStripesBtn:focus, .blueStripesBtn:active {
    background-image:url(/Content/images/stripespattern6.png);
}


.reportLink {
    margin-top: 20px;
    background-color: #000;
    color: #fff;
    padding: 5px;
}

.reportLink:hover, .reportLink:focus, .reportLink:active {
    color:#fff;
    text-decoration:none;
    background-color: rgb(18, 157, 183);
} 

.caretHolder {
    display:inline-block;
    width:20px;
}

.caret {
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 4px solid #000000;
    display: inline-block;
    height: 0;   
    width: 0;
    margin: 10px;
    vertical-align:middle;
    
}

.right-caret {
    border-bottom: 4px solid transparent;
    border-top: 4px solid transparent;
    border-left: 4px solid #000000;
    display: inline-block;
    height: 0;
    width: 0;
    margin: 10px;
    vertical-align:middle;
}

.btn .caret {
    margin-left: 5px;
}

.scrollableDiv .caret {
    border-top: 4px solid #fff;
}

.scrollableDiv .right-caret {
    border-left: 4px solid #fff;
}

a {
    cursor:pointer;
}

.positionRelative {
    position:relative;
}


input[type="text"], input[type="password"], input[type="email"], button, .btn, select, textarea {
    border-radius:0 !important;
    outline: 0 !important;
    -webkit-box-shadow:none !important;
    box-shadow:none !important;
    border-top-left-radius: 0 !important;
    border-top-right-radius: 0 !important;
    border-bottom-left-radius:0 !important;
    border-bottom-right-radius: 0 !important;
}

input[type="text"], input[type="password"], input[type="email"], textarea {
    -webkit-appearance: none !important;
}

input[type="text"], input[type="password"], input[type="email"] {
    height:50px;
    line-height:50px;
}

.check-mark:not(#foo) > input[type='checkbox'] {
    opacity: 0;
    float: left;
}
.check-mark:not(#foo) > input[type='checkbox'] + label {
    margin: 0;
    clear: none;
    padding: 5px 0 4px 50px;
    cursor: pointer;
    min-height: 32px;
    background: url('/Content/images/checkbox_32x32.png') left center no-repeat;
}
.check-mark:not(#foo) > input[type='checkbox']:checked + label {
    background-image: url('/Content/images/checkbox_checked_32x32.png');
}



.form-control {
    -webkit-box-shadow:none;
    box-shadow:none;
}

.placeholder { 
    color: #aaa; 
}

.print {
    display:none;
}

.mobile {
    display:none;
}

.desktop {
    display:block;
}

.transitionElement {
    opacity:0;
}

.tableCell {
    display: table-cell;
    vertical-align:middle;
}

.modal {
    outline:0;
    z-index:10000;
}

.smallPrint {
    font-size:12px !important;
}

p.generalP {
    font-size:15px;
    font-family:'open sans';
}

.radio {
    padding-left:5px;
}

.radio label {
    width: 100%;
    display: inline-block;
    padding: 5px 0;
}

.img-responsive {
    margin:0 auto;
}


.classificationCrumbs {
    margin-bottom: 10px;
    font-size: 20px;
}

.dropdown-toggle {
    background-color: transparent;
    border: 2px solid #27AEC0;
    font-weight: 600;
    text-transform: uppercase;
    padding: 0 2px 0 8px;
    line-height: 50px;
    height: 50px;
}

.dropdown-toggle:hover, .dropdown-toggle:focus, .dropdown-toggle:active, .open .dropdown-toggle.btn-default {
    color: #000;
    background-color: rgba(23, 184, 214, 1) !important;
    border: 2px solid #27AEC0 !important;
}

.dropdown-menu > li > a {
    padding: 9px 20px;
    border-bottom: 1px solid #f0f0f0;
}

.dropdown-menu {
    padding:0;
}


.groupby {
background-color: #fff;
padding: 0;
}

.activeLi {
background-color: rgb(222, 242, 247);
}

.groupby>li>a {
padding: 9px 20px;
border-bottom: 1px solid #f0f0f0;
}

.desktopBtn {
    text-align:right;
}

.career {
    font-size: 19px;
    color: #000;
    padding: 10px 5px;
    display: block;
    min-height: 50px;
    cursor: pointer;
    position:relative;
}

.career:hover, .career:focus, .career:active {
    background-color: rgb(222, 242, 247);
    -webkit-tap-highlight-color: rgb(222, 242, 247);
    text-decoration: none;
}

.professionName {
    padding-right: 28px;
    width: 100%;
    display: block;
}

 .arrowRight {
    position: absolute;
    right: 0;
    top: 50%;
    font-size: 20px;
    margin-top: -14px;
    line-height:initial;
}

 .previouslyChosen:after {
    content: '*';
    color: rgb(227, 84, 42);
    font-size: 20px;
    padding-left: 6px;
}



@media only screen and (max-width: 1200px) {
    .career {
    font-size: 14px;
    min-height: 40px;
    text-decoration: none;
    }
}



/* CUSTOMIZE THE NAVBAR
-------------------------------------------------- */


.navbar-inverse {
    background-color: rgb(0,0,0);
    border: none;
}

/*brand nav*/
.navbar-inverse .navbar-brand {
    padding: 5px 10px;
    margin-left:0 !important;
    position:relative;
    padding: 15px 15px 15px 0;
}

#logoicon {
    height: 50px;
    position: absolute;
    top: 0;
    padding: 5px 8px;
}

#logoxtext {
    height: 14px;
    padding-left: 60px;
}

.navbar-inverse .navbar-brand:hover, .navbar-inverse .navbar-brand:focus {
    color: #FF7D5C;
}

/*active nav*/
 .navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:hover, .navbar-inverse .navbar-nav>.active>a:active, .navbar-inverse .navbar-nav>.active>a:focus {
    background-color: rgb(143, 214, 228);
    color: #000;
}


/* Special class on .container surrounding .navbar, used for positioning it into place. */
.navbar-wrapper .navbar {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    z-index: 20;
    border-radius:0;
    border-radius:0 !important;   
}

.navbar-wrapper {
    position:relative;
}

.navbar-nav {
    margin: 0;
    padding: 0;
}

.navbar-wrapper .container {
    padding:0;   
}

/*move menu button to the left on collapsed screen*/
.navbar-toggle {
    float:left;   
    margin-left:5px;
    display:none;
}

 .navbar-toggle:hover, .navbar-toggle:focus {
    background-color: rgb(143, 214, 228);
    border-color: #333;   
    outline-color: #333;
    color: #000;
}

.navbar-inverse .navbar-nav > li > a {
    color: #FFF;
    font-size: 26px;
    text-transform: uppercase;
    font-weight: 600;
    font-family: 'open sans condensed';
    padding:17px 10px 13px;
}

 .navbar-inverse .navbar-nav>li>a:hover, .navbar-inverse .navbar-nav>li>a:active, .navbar-inverse .navbar-nav>li>a:focus {
    color: #FF7D5C;   
}

 .navbar-nav.navbar-right:last-child {
     margin-right:0;
 }

/*fix conflict of icon with fontello*/
.icon-bar:before {
    content: none;
}

/*search box*/
#masterNav .btn {
    width:80px;    
}  
    
/*search inside menu*/    
#masterNavSearch {
    width: 300px;
    height:40px;
    padding:10px 15px;
    float:right;
}

#searchbox {
    height: 50px;
    font-size: 16px;
    width: 60%;
    padding:10px;
    /*background-color: #EDEDF5;*/
    /*transition: width 1s ease-out;
    webkit-transition: width 1s ease-out;*/
    outline:0;
    border: thin solid #ddd;
}

#searchbox::-webkit-input-placeholder {
    font-style: italic;
}

#searchbox::-moz-placeholder {
    font-style: italic;
}
#searchbox:-moz-placeholder {
    font-style: italic;
}
#searchbox:-ms-input-placeholder {
    font-style: italic;
}

#searchbox:focus {
    border-color: rgba(23, 184, 214, 1);
    /*transition: width 1s ease-out;
    webkit-transition: width 1s ease-out;*/
}
   
    
/*search box that shows up underneath menu on mobile*/
/*not collapsed*/
#underNavSearch {
    display:none;
    position: absolute;
    top:50px;
    left:0;
    right:0;
    padding: 2% 8%;
}
    

.loginDisplay, .logoutDisplay {
   color:#C6D7DB;
    padding: 16px 10px 12px;
    font-size: 16px;
}


.logoutDisplay a {
    color: #C6D7DB !important;
    font-size: 16px !important;
    text-transform: none !important;
    font-weight: 400 !important;
    font-family: 'open sans' !important;
    padding: 0 !important;
}

.logoutDisplay a:hover, .logoutDisplay a:focus, .logoutDisplay a:active {
    text-decoration:underline !important;
}



/* BACK TO TOP ICON
-------------------------------------------------- */
#back-top {
display:none;
position: fixed;
right: 60px;
bottom: 30px;
z-index: 999;		
width: 50px;
height: 50px;
background-color: rgb(160,160,160);
background-color: rgba(160,160,160,.9);
text-align: center;
-webkit-transition: all .25s ease;
transition: all .25s ease;
-webkit-border-radius: 50%;
border-radius: 50%;
text-decoration:none;
}
	

.no-touch #back-top:hover {
border-color: transparent;
background-color: #000;
}
	
#back-top:after {
width: 40px;
height: 40px;
text-align: center;
color: #e1e1e1;
font-size: 20px;
font-family: 'fontello';
content: "\ed4e";
line-height: 48px;		
-webkit-transition: all .25s ease;
transition: all .25s ease;
}
		
.no-touch #back-top:hover:after {
color: #fff; 
}
	
#back-top a {
text-decoration: none; 
background-color: transparent;  
font-size:0;
color:transparent;
}

.no-touch #back-top a:hover {
text-decoration: none; 
background-color: transparent;  
font-size:0;
color:transparent; 
}
	


