.mobileTestModal .modalTitle {
    background-color: #000;
    height: 50px;
    color: #fff;
    line-height: 50px;
    margin:0;
}

.mobileTestModal .steps {
    position: absolute;
    top: 50px;
    left: 0px;
    right: 0px;
    padding:10px;
}

.mobileTestModal p {
    color: #000;
    font-size: 17px;
    font-family: 'open sans';
    text-transform: uppercase;
    white-space: initial;
}

.mobileTestModal #slide1 p, .mobileTestModal #slide2 p {
    font-size: 22px;
    font-family: 'open sans';
    text-transform: uppercase;
    white-space: initial;
}

.mobileTestModal #slide3 p {
    color: #000;
    font-size: 17px;
    font-family: 'open sans';
    text-transform: uppercase;
    white-space: initial;
}

.mobileTestModal #slide3 #selectedCareer {
    font-family: 'open sans';
    color: #666;
    margin: 5px;
    font-size: 14px;
    text-transform: none;
}

.mobileTestModal #pickProfessionBtn {
    position:relative;
    font-family: 'open sans condensed';
    font-weight: 600;
    text-transform: uppercase;
    padding: 8px;
    white-space: initial;
}

.mobileTestModal #pickProfessionBtn i.fa-angle-right {
    display: block !important;
    font-size: 18px;
    font-weight: 600;
    margin-top: -12px;
}

.mobileTestModal #pickProfessionBtn i.fa-angle-right:before {
    font-weight:600;
}

.mobileTestModal .stepsHeader {
    text-align: center;
    font-size: 18px;
    padding: 5px;
    background-color: rgba(23, 184, 214, 1);
    margin-bottom:20px;
}

.mobileTestModal .questionContent {
    width: 100%;
}

.mobileTestModal .questionContent .row {
    margin: 10px 0 0;
}

.mobileTestModal .questionContent .col-lg-12 {
    padding: 0 5px;
}


.mobileTestModal .startNewTest {
    margin-top: 10px;
}

.mobileTestModal .jobs {
    overflow-x: hidden;
}
.mobileTestModal .menuWrap {
    background-color: white;
    top: 0;
    left: 0;
    right: 0;
}

.mobileTestModal #searchJob {
    margin-top: 5px;
    margin-left: 5px;
}

.mobileTestModal .closeIcon {
    top: 5px;
}

.mobileTestModal .resultBtnMobile {
    width: 200px;
    height: 50px;
    line-height: 48px;
    vertical-align: middle;
    bottom: 70px;
    padding: 0;
    background-color: #38D5A3;
    border-radius: 0;
    border: 2px solid #61BE80;
    color: #fff;
    font-size: 18px;
    cursor: pointer;
    position: fixed;
    left: 50%;
    margin-left: -100px;
}

.mobileTestModal .resultBtnMobile:hover, .resultBtnMobile:focus, .resultBtnMobile:active {
    color: #000;
    border-color: #000;
}


