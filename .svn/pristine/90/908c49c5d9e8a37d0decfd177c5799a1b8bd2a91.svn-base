/*===== images settings and z-indexes =====*/
img {
    position: absolute;
}

.characters img {
    opacity: 0;
}

#background {
    transition: filter 0.5s ease-in-out;
    -webkit-transition: -webkit-filter 0.5s ease-in-out;
    -moz-transition: -moz-filter 0.5s ease-in-out;
    -o-transition: -o-filter 0.5s ease-in-out;
    z-index: 10;
}

.gray {
    filter: grayscale(100%);
    filter: gray;
    -webkit-filter: grayscale(1);
    -moz-filter: grayscale(1);
    -ms-filter: grayscale(1);
    -o-filter: grayscale(1);
}

.leftPerson {
    z-index: 11;
}

.rightPerson {
    z-index: 12;
}

.left_1 {
    z-index: 13;
}

.right_1 {
    z-index: 14;
}

.center_1 {
    z-index: 15;
}

#blank {
    z-index: 16;
}

area, area:active, area:focus {
    outline: none;
    border: none;
}
/*===== END image settigns =====*/

/*===== text settigns =====*/

div {
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

.headersText {
    text-align:center;
}

#questionText, h1 {
