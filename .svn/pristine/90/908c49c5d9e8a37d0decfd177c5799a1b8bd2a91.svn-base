/***************nav menu********************/
*, *::before {
    box-sizing: border-box;
}


.bt-menu {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 0;
	border-width: 0px;
	border-style: solid;
	border-color: #333;
    border-color: #252628;
    background-color: rgb(0,0,0);
	background-color: rgba(0,0,0,0);
	-webkit-backface-visibility: hidden;
	-webkit-transition: border-width 0.3s, background-color 0.3s, height 0s 0.3s;
	transition: border-width 0.3s, background-color 0.3s, height 0s 0.3s;
}

.bt-menu.bt-menu-open {
	height: 100%;
	border-width: 10px 90px 30px 90px;
    background-color: rgb(0,0,0);
	background-color: rgba(0,0,0,0.3);
	-webkit-transition: border-width 0.3s, background-color 0.3s;
	transition: border-width 0.3s, background-color 0.3s;
}

.bt-overlay {
	position: absolute;
	width: 100%;
}

.bt-menu-open .bt-overlay {
	height: 100%;
}

/*menu sandwich icon*/
.bt-menu-trigger {
	position: fixed;
	top: 15px;
	right: 15px;
	z-index: 100;
	display: block;
	width: 60px;
	height: 60px;
	cursor: pointer;
    background-color: #252628;
    border: 5px solid #252628;
    border-radius: 10px;
}

/*create center line*/
.bt-menu-trigger span {
	position: absolute;
	top: 50%;
	left: 0;
	display: block;
	width: 100%;
	height: 4px;
	margin-top: -2px;
	background-color: #fff;
	font-size: 0px;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-transition: background-color 0.3s;
	transition: background-color 0.3s;
}

/*center line disappears when menu is open*/
.bt-menu-open .bt-menu-trigger span, .open .bt-menu-trigger span {
	background-color: transparent;
}

/*common styles for upper and lower line*/
.bt-menu-trigger span:before,
.bt-menu-trigger span:after {
	position: absolute;
	left: 0;
	width: 100%;
	height: 100%;
	background: #fff;
	content: '';
	-webkit-transition: -webkit-transform 0.3s;
	transition: transform 0.3s;
}

/*position upper line higher from center*/
.bt-menu-trigger span:before {
	-webkit-transform: translateY(-250%);
	transform: translateY(-250%);
}
/*position lower line below center line*/
.bt-menu-trigger span:after {
	-webkit-transform: translateY(250%);
	transform: translateY(250%);
}

/*when menu is open rotate upper line 45 deg*/
.bt-menu-open .bt-menu-trigger span:before, .open .bt-menu-trigger span:before {
	-webkit-transform: translateY(0) rotate(45deg);
	transform: translateY(0) rotate(45deg);
}

/*when menu is open rotate lower line -45deg*/
.bt-menu-open .bt-menu-trigger span:after, .open .bt-menu-trigger span:after {
	-webkit-transform: translateY(0) rotate(-45deg);
	transform: translateY(0) rotate(-45deg);
}


#leftside {
	position: fixed;
	top: 10px;
	left: 0;
}


#rightside {
    position:fixed;
    right: 0px;
    top: 85px;
}

.bt-menu ul {
	margin: 0;
	padding: 0;
	width: 90px;
	list-style: none;
	text-align: center;
	white-space: nowrap;
	height: 0;
	-webkit-transition: height 0s 0.3s;
	transition: height 0s 0.3s;
    	-webkit-backface-visibility: hidden;
	backface-visibility: hidden;
}

.bt-menu ul li,
.bt-menu ul li a {
	text-align: center;
    display:block;
}


.bt-menu ul li {
	padding: 0;
	opacity: 0;
	visibility: hidden;
	-webkit-transition: -webkit-transform 0.3s, opacity 0.2s, visibility 0s 0.3s;
	transition: transform 0.3s, opacity 0.2s, visibility 0s 0.3s;
    margin:10px;
}


.bt-menu.bt-menu-open ul li {
    visibility:visible;
	opacity: 1;
	-webkit-transform: translate3d(0,0,0);
	transform: translate3d(0,0,0);
}

.bt-menu.bt-menu-open ul li:first-child,
.bt-menu.bt-menu-open ul li:last-child {
	-webkit-transition-delay: 0.15s;
	transition-delay: 0.15s;
}

.bt-menu.bt-menu-open ul li:nth-child(2),
.bt-menu.bt-menu-open ul li:nth-last-child(2) {
	-webkit-transition-delay: 0.1s;
	transition-delay: 0.1s;
}

.bt-menu.bt-menu-open ul li:nth-child(3),
.bt-menu.bt-menu-open ul li:nth-last-child(3) {
	-webkit-transition-delay: 0.05s;
	transition-delay: 0.05s;
}


.bt-menu ul li button {
	display: block;
	outline: none;
	text-decoration: none;
	text-transform: uppercase;
	letter-spacing: 1px;
	-webkit-transition: color 0.2s;
	transition: color 0.2s;
}

.bt-menu ul li button:before {
	display: none;
	font-size: 32px;
}



