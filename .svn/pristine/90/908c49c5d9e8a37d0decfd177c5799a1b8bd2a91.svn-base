/*===== hover and selected tooltip =====*/
.hover-tooltip {
    position: absolute;
    background-color: #fff;
    z-index: 24;
    cursor: default;
    padding: 10px;
    border-radius: 14px;
    -moz-box-shadow: 3px 3px 4px 4px #414040;
    -webkit-box-shadow: 3px 3px 4px 4px #414040;
    box-shadow: 3px 3px 4px 4px #414040;
}

.hover-tooltip.top .arrow::after {
    border-top-color: #fff;
}

.selected-tooltip {
    position: absolute;
    background-color: #6CD3C4;
    z-index: 23;
    cursor: default;
    padding: 10px;
    border-radius: 14px;
    -moz-box-shadow: 3px 3px 4px 4px #414040;
    -webkit-box-shadow: 3px 3px 4px 4px #414040;
    box-shadow: 3px 3px 4px 4px #414040;
}

.selected-tooltip.top .arrow::after {
    border-top-color: #6CD3C4;
}

.popover-content {
padding: 12px 14px;
color: #000;
font-family: 'open sans';
font-size: 16px;
}
/*===== END hover and selected tooltip =====*/


/*===== desktop navigation controls =====*/
#navigationWrapper {
    position: absolute;
    width: 100%;
    bottom: -60px;
    text-align: center;
    z-index: 19;
    display: none;
}

.slideOutWrapper {
    position:relative;
    height: 30px;
    text-align: center;
    overflow: hidden;
    border: none;
    outline: none;
}

.slideOut, .slideOut:focus, .slideOut:active {
    position: relative;
    background-color: rgb(238,238,238);
    background-color: rgba(238,238,238, 0.7);
    text-align: center;
    width: 70px;
    height: 70px;
    margin: auto;
    margin-bottom: -40px;
    border-radius: 50%;
    border: none;
    outline: none;
    z-index: 25;
    float: none;
}

.slideOut:after {
    z-index: 26;
}

.container, .container:focus, .container:active {
    text-align: center;
    position: relative;
    margin: 0 auto;
    padding-top: 10px;
    height: 60px;
    background-color: rgb(238,238,238);
    background-color: rgba(238,238,238, 0.7);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    z-index:26;
    border: none;
    outline: none;
}

.btn-toolbar {
    float: none !important;
}

.continueWrapper {
    position: fixed;
    bottom: 200px;
    width: 100%;
    text-align: center;
    z-index: 20;
}
/*===== END desktop navigation controls =====*/

