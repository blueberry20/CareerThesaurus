
#careersIndex .mainContainer {
    background-color: #fff;
    padding:20px 0;
}

#main {
    padding:0;
}

#careersIndex .importantthings,#careersIndex #dimensionscontainer {
    display:none;
}

#mainWrapper {
    width:100%;
}

#mainWrapper h2 {
    text-align:left;
    margin:0;
}

.jumbotron {
    margin-bottom:0;
}

.imageStretch {
    background-image: url('topviewstudents_cropped.jpg');
    background-size:cover;
    height:700px;
    position:relative;
}

.takeTestJumbotron {
    position: relative;
}

 .taketestJumbotron h1 {
    bottom:60px;
}

 .testBanner {
     margin:20px;
 }


 .bannerText p {
    font-size: 20px;
    background-color: rgba(255,255,255,0.6);
    background-color: rgba(0,0,0,0.6);
    text-transform: uppercase;
    font-weight: 300;
    margin: 5px;
    color: #fff;
    padding: 5px;
}

 .text1 {
    position:absolute;
    top: 0;
    background-color: #000;
    background-color: rgba(0,0,0,0.6);
    color: #fff;
    font-size: 24px;
    padding: 5px;
    text-transform: uppercase;
    font-family: 'open sans condensed';
 }

 .text2 {
    position:absolute;
    bottom: 0;
    background-color: #fff;
    background-color: rgba(255,255,255,0.8);
    font-size: 19px;
    padding: 5px;
    text-transform: uppercase;
    margin:0;
 }


.testBanner .btn-success {
    margin-top: 5px;
    width: 100%;
    display: block;
}

.choiceJumbotron {
    background-color: #C6E1E4;
    padding:20px;
}


.parallax-bg-3 .full-bg-image {
    background-image: url('/Content/images/hex/hex_bg9.jpg');
}

.parallax { 
    z-index: 0;
    background-color: transparent;
    position:relative;
}
		
/* BG Overlay Grid  */
		
.parallax-overlay {
    position: absolute;
    z-index: 0;
    left: 0;
    top: 0;
    margin: 0 auto;
    width: 100%;
    height: 100%;
    background-color:rgba(189, 247, 253, 0.6);
}

    
/* BG Image */
		
.full-bg-image {
    position: absolute;
    z-index: -1;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-position: 50% 50%;
    background-repeat: repeat;		
    background-attachment: fixed;
    background-size: cover;
}

.choiceJumbotron .col-lg-6 {
    position:relative;
}

.choiceJumbotron h1 {
    font-size: 50px;
    text-align: center;
    margin: 20px;
    font-weight: 600;
    font-family:'open sans condensed';
    position:relative;
    color:#000;
}


.optionDiv {
    width: 80%;
    text-align: center;
    margin: 0 auto;
    border-radius: 5%;
    position:relative;
    padding:20px;
}


.optionDiv h2 {
    position: absolute;
    top: 0;
    color: #000;
    padding: 0 5px;
    font-size: 42px;
    font-weight: 600;
    font-family:'open sans condensed';
    background-color:#fff;
}

.circleGoBtn {
    width: 100px;
    height: 100px;
    border-radius: 50%;
    background-color: #38D5A3;
    text-align: center;
    line-height: 100px;
    position: absolute;
    right: 30px;
    bottom: 30px;
    font-weight: 600;
    font-family: 'open sans condensed';
    font-size: 30px;
    cursor:pointer;
    text-decoration:none;
    color:#000;
}

.circleGoBtn:hover, .circleGoBtn:focus, .circleGoBtn:active {
    text-decoration:none;
    color:#fff;
}


.optionDiv h3 {
    font-size: 32px;
    font-weight: 600;
    font-family: 'open sans condensed';
    text-transform: uppercase;
    position: absolute;
    top: 20px;
    background-color: #33CADE;
    padding: 0;
}

.centerOption {
    padding: 0;
    width: 100%;
}

.searchboxWrapper {
    padding:0 15px 15px;
}

.sortedInfo {
    margin: 10px 15px;
}

@media only screen and (max-width:1200px) {

    #careersIndex .container {
