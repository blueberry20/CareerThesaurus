/*--================================= HOME PAGE CSS ============================== */
body {
    overflow-x: hidden;
}

h1 {
    font-size: 92px;
    font-weight:700;
    color:#fff;
}

 h2 {
    font-size: 40px;
    color: #fff;
    text-transform: none;
    font-family: 'roboto';
    margin: 50px 0 30px;
    font-weight:300;
}

 
h3 {
    margin: 0;
    font-size: 34px;
    font-weight: 600;
    font-family: 'roboto';
    color: #fff;
    margin-bottom:20px;
}

section h2 {
	margin: 20px 0 40px;
	padding: 0;
    color:#fff;
    font-weight: 700;
    font-size: 70px;
    text-transform: uppercase;
}

section {
	position: relative;
	padding: 0;
	background: #fff;
	color: #fff;
	text-align: center;
    background-color:transparent;
    display:table;
    overflow:hidden;
    min-height:760px;
    width:100%;
    border-bottom:1px solid #fff;
}

section:last-child {
    border-bottom:none;
}

.section .bottomArrow, section .bottomArrow {
    position: absolute;
    bottom: 0;
    color: #fff;
    font-size: 70px;
    left: 50%;
    margin-left: -22px;
    font-weight: 300;
    cursor:pointer;
}

.shortSection {
   min-height:initial;
    width: 100%;
    padding: 100px 0;
    color: #000;
}

.shortRow {
    width:90%;
    margin: 0 auto;
    height:120px;
}

.nowrap {
    white-space:nowrap;
}


/* Common style for pseudo-elements */
section::before,
section::after {
	position: absolute;
	content: '';
	pointer-events: none;
}

section .img-responsive {
    position:absolute;
    left:0;
    top:0;
    width:100%;
}

section .container {
    opacity:0;
    top:30px;
    position:absolute;
    -webkit-transition: all 1s ease-in;
    -moz-transition: all 1s ease-in;
    -ms-transition: all 1s ease-in;
    -o-transition: all 1s ease-in;
    transition: all 1s ease-in;
    width: 100%;
    top: 0;
    /*background-color: rgba(0,0,0,0.3);*/
    left: 0;
    /* margin-left: -300px; */
    height: 100%;
    padding:200px 25px;
    display:table-cell;
    vertical-align:middle;
}

section .container.viewed{
    opacity:1;
    top:0;
}

input[type="text"], input[type="password"], input[type="email"] {
    padding: 0 15px !important;
    border: 4px solid #fff;
    border:4px solid rgba(255,255,255,0.7);
    background-color: rgb(189, 236, 234);
    background-color: rgba(189, 236, 234, 0.9);
    color:#000;
}

.form-control:focus {
    border-color: #fff;
    background-color: #fff;
    background-color: rgba(255,255,255,0.7);
}

.form-control:focus::-webkit-input-placeholder {
   color: #000;
}

.form-control:focus:-moz-placeholder { /* Firefox 18- */
   color: #000;  
}

.form-control:focus::-moz-placeholder {  /* Firefox 19+ */
   color: #000;  
}

.form-control:focus:-ms-input-placeholder {  
   color: #000;  
}

/* Triangles */
.ss-style-triangles::after {
	left: 50%;
	width: 100px;
	height: 100px;
	-webkit-transform: translateX(-50%) rotate(45deg);
	transform: translateX(-50%) rotate(45deg);
    bottom:-50px;
    z-index:0;
    position:absolute;
    background: inherit;
}


/*.ss-style-triangles::after {
	bottom: -50px;
	z-index: 10;
	background: inherit;
}*/



/* Big Triangle */
svg#bigTriangleColor {
	pointer-events: none;
}

#bigTriangleColor path {
	fill: #67E8F3;
	stroke:#67E8F3;
	stroke-width: 2;
}

.bottom {
    position: absolute;
    bottom: 0;
    left:0;
    height: 75px;
    overflow:hidden;
    /* background-color: #fff; */
    width: 100%;
    border-left: 805px solid #67E8F3;
    border-right: 898px solid #67E8F3;
    border-top: 0 solid transparent;
}

#arrow {
    position: absolute;
    left: 50%;
    margin-left: -100px;
    height: 100px;
    /* width: 200px; */
    bottom: 0;
    border-left: 100px solid #fff;
    border-right: 100px solid #fff;
    border-top: 100px solid transparent;
}

