@media only screen and (max-width : 1400px) {


    section .container {
        padding: 100px 25px;
    }

    .rowColumn {
        padding: 50px 20px 0;
    }

    #testimonialSection .fa-angle-left {
        left:50px;
    }

    #testimonialSection .fa-angle-right {
        right:50px;
    }


} 

@media only screen and (max-width: 1300px) {
    section h2 {
        font-size:60px;
    }
}

@media only screen 
and (max-width : 1200px) {

  h1 {
        font-size: 80px;
    }

    .searchJobsBtn, .goBtn {
        width:200px;
        height:50px;
        line-height: 42px;
    }

    section h2, .justText h2 {
        font-size: 50px;
        margin-bottom: 30px;
    }


    .rowColumn p {
        font-size: 18px;
    }

    #testimonialSection .fa-angle-left {
        left:50px;
    }

    #testimonialSection .fa-angle-right {
       right:50px;
    }

} 

/*hide one row of images when they get to 3 per row*/
@media only screen 
and (max-width : 992px) {

     h1 {
        font-size: 62px;
    }

    .frontpageimageContent .sectionContent {
        margin-top: 20%;
    }

    .section .bottomArrow, section .bottomArrow {
        font-size:50px;
        margin-left:-16px;
    }

    section h2, .justText h2 {
        font-size: 40px;
        margin-bottom: 30px;
    }

    .shortRow {
        width: 90%;
    }

    .rowColumn p {
        font-size: 16px;
    }

    i.fa.fa-quote-left, i.fa.fa-quote-right {
        padding: 0 5px;
        font-size: 12px;
        color: #A3A1A1;
    }

} 


@media only screen and (max-width: 768px) {
    #careersSection .row div:nth-child(4), #careersSection .row div:nth-child(3) {
        display: none;
    }

    h1 {
        font-size:50px;
    }

    .frontpageimageContent .sectionContent {
        margin-top: 36%;
    }


    .searchJobsInput, .zipcodeInput {
        width: 80%;
    }

    #testimonialSection .fa-angle-left {
        left:-20px;
        top:60%;
    }

    #testimonialSection .fa-angle-right {
       right:-20px;
       top:60%;
    }

    #smallFont {
        margin: 20px auto 0;
        font-weight: 300;
        width: 90%;
    }

    .divider {
        width: 200px;
        border: 1px solid #66CED8;
        margin: 30px auto 0;
    }

    .testimonialRow {
        position:relative;
    }

    .shortRow {
        min-height:130px;
    }

    section {
        min-height:600px;
    }

}


