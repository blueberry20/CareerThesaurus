/* Counter Box
/* -------------------------------------------------- */			
#counterDiv {
    min-height:initial;
    background: url('/Content/images/p6.png') repeat;
}

#counterDiv .container {
    opacity:1;
}

#counterDiv h2 {
    width:100%;
}


.counter-box p, .counter-box span {
    font-size: 24px;
    color: #666;
    text-transform: uppercase;
}

.counter-box p {
    margin:15px 0;
}

.counter-box  {
    text-align: center; 
    margin-top:60px;
}

.counter, .counter3 {
    display: block;
    text-align: center;
    position:relative;
}

#thinArrow {
    position: absolute;
    left: -26px;
    width: 50px;
    top: 50%;
}

.counter .count, #bluebox {
    margin-bottom: 20px;
    height: 150px;
    border: 1px solid #c8c8c8;
    border: 1px solid #5EE3F5;
    display: block;
    background-color: rgb(255,255,255);
    background-color: rgba(12, 204, 228, 0.7);
    color: #000;
    font-weight: 300;
    font-size: 33px;
    font-family: 'Roboto Condensed', sans-serif;
    line-height: 200px;
    -webkit-border-radius: 6px;
    border-radius: 6px;		
    margin:0 auto;
    width:200px;
    height:200px;
}   

#bluebox {
    font-size:16px;
    line-height:initial;
    margin-top: 59px;
}

#bluebox span {
    vertical-align: middle;
    padding-top: 76px;
    display: block;
    font-size: 20px;
    font-family: 'roboto';
    color: #000;
    font-weight: 600;
}


#sectionGetCareer p {
    margin-top: 20px;
    font-size: 20px;
    font-weight: 300;
}

