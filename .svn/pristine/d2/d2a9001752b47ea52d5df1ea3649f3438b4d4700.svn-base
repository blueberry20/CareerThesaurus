.letterSection {
background-color: #fff;    
}

/*letter*/
.letterSection h2 {
padding:5px; 
padding-left:20px;
background-color: #A4A4A4;  
}

/*remove boostrap side padding*/
.listing .row div {
padding:0;
}


/*override default boostrap margin for row*/
#careerListingsSection .row {
margin:0;
}

/*career listings common styles*/
.listing {
padding:20px;
cursor:pointer;
}

.listing h3 {
padding-bottom:0;   
margin: 0 0 20px;
}

.listing .col-lg-2,.listing .col-md-2,.listing .col-sm-2,.listing .col-xs-2 {
text-align:right;
/* Firefox */
display:-moz-box;
-moz-box-pack:center;
-moz-box-align:center;
/* Safari, Opera, and Chrome */
display:-webkit-box;
-webkit-box-pack:center;
-webkit-box-align:center;
/* W3C */
display:box;
box-pack:center;
box-align:center; 
}

.listing p {
margin:0;   
}


