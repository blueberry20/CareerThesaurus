
/* CAREERS IN PAGE
-------------------------------------------------- */

#careersInPage {
    padding-top:70px;
}

@media only screen
and (max-width: 1250px) {
    #careersInPage .container {
    padding-right:50px;   
    }
}

@media only screen 
and (max-width : 814px) {
    #careersInPage {
    padding-right:0px;   
    }
       
    #careersInPage .container {
    padding-right:30px;   
    }

    .navbar-brand {
        display:none;
    }
}


/***************Alphabetical Index*******************/

#alphabetIndex {
margin:0;   
width:50px;
position: fixed;
right: -50px;
top: 51px;
z-index: 11000;
height:90%;
background-color:rgb(255,255,255);
background-color:rgba(255,255,255,0.8);
-webkit-transition-duration: 0.3s;
-moz-transition-duration: 0.3s;
-o-transition-duration: 0.3s;
transition-duration: 0.3s;
border:1px solid #666;
border-radius:6px;
}

/*alphabetical sidebar overrides*/
#alphabetIndex .list-group {
margin:0;  
height:100%; 
background-color:transparent;
padding-top:3px;
}

#alphabetIndex .list-group-item:first-child, .list-group-item:last-child {
border-radius:4px;   
}

/*cell that holds a letter*/
#alphabetIndex .list-group-item {
height: 3.84%;   
margin:0;
padding:0;
text-align:center;
color:#000;
font-size:14px;
border: none;
background-color:transparent;
}

#alphabetIndex a.list-group-item {
color: #000;   
background-color:transparent;
}

a.list-group-item.active, 
.no-touch a.list-group-item.active:hover, 
a.list-group-item.active:focus {
background-color: #a4a4a4;
border-color: #a4a4a4;   
}

#alphabetIndex .list-group-item p {
background-color:transparent;
margin:0;
}

.no-touch #alphabetIndex .list-group-item p:hover {
font-size: 22px;
color: #000;   
}


/*make alphabet index smaller on mobile*/
@media only screen 
and (max-width : 992px) {
    #alphabetIndex {
    width: 30px; 
    background-color: rgb(255,255,255);
    background-color: rgba(255,255,255,0.7);
    border-radius: 8px; 
    position:fixed;        
    right:0; 
    }
    
    #abc {
    display:none;   
    }
    
   .no-touch #alphabetIndex .list-group-item p:hover {
    font-size: 18px; /*make font smaller for mobile*/  
    }
    
    #careersInPage .container {
    padding-left:0px;   
    }
    
    #careersInPage {
    padding-bottom:0;   
    margin-right:0;
    }
    
    #careersInPage h1 {
    padding-left:20px;   
    }  
}

#abc {
position:fixed;
right:0px;
top:45%;
padding: 5px;
font-size:20px;
letter-spacing:5px;
background-color:#000;
color:#fff;
z-index: 9999;
border: 1px solid gray;
border-radius:8px 8px 0 0;
-webkit-transition-duration: 0.3s;
-moz-transition-duration: 0.3s;
-o-transition-duration: 0.3s;
transition-duration: 0.3s;
text-align:center;
/* Safari */
-webkit-transform-origin: 100% 100%;
-webkit-transform: rotate(-90deg);

/* Firefox */
 -moz-transform-origin: 100% 100%;
-moz-transform: rotate(-90deg);

/* IE */
-ms-transform-origin: 100% 100%;
-ms-transform: rotate(-90deg);

/* Opera */
-o-transform-origin: 100% 100%;
-o-transform: rotate(-90deg);

/* Internet Explorer */
filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);

}

#abc ul li{
list-style: none;
padding:5px 15px;
margin:0 auto;
}

#abc ul {
padding:0;   
}

.no-touch #indexHolder:hover #abc {
right:50px;   
}

.no-touch #indexHolder:hover #alphabetIndex {
right:0;   
}



