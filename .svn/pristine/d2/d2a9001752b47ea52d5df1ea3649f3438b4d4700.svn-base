/********************************MOBILE. TAGS OVERLAY******************/

/*overlay of tags on click of icon*/
#mobileCareerTags {
display:none;
background-color:rgb(0,0,0);
position:fixed;
left:0;
top:0;
height:100%;
width:100%;   
z-index:11000;
padding:0px;
text-align:center;
margin-top:50px;
}

#mobileCareerTags h1 {
color:#fff;
text-align:center;
font-size:20px;   
margin:20px 0;
}

#mobileCareerTags h2 {
color: #fff; 
padding:5px;
margin:20px;
text-transform:uppercase;
}


.scrollableDiv { 
overflow:auto;
}

#mobileCareerTags .scrollableDiv {
height: 80%; 
padding-bottom:100px;
}

    
.glyphicon-remove {
color: #ff7d5c;
position:absolute;
top:-5px;
right:10px;  
font-size:10px;
}
    

#mobileCareerTags .tag:after {
content:" +";
background-color: rgb(0,0,0);
background-color: rgba(0,0,0,0.9);
text-decoration:none;
color:#fff;
}

#mobileCareerTags hr {
margin: 10px;   
}

.mobileKeyword {
border:1px solid #fff;
border-radius:40px;
font-size:16px;
color:#fff;   
padding:10px;
margin: 5px;
display:inline-block;
cursor: pointer;
}

.no-touch #mobileCareerTags a:hover {
text-decoration:none;
color: #fff;   
}

.selected {
background-color:#39b3d7;
border-color: #269abc;
}

