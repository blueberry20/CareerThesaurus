.pageContainer {
    padding: 50px 0 0;
}

.whiteBgPage .container {
    background-color: #fff;
    padding: 20px 30px 30px;
}

#studentReportPage h1 {
    text-align:left;
    display:block;
    margin-top:20px;
}

.pageContainer h2 {
    margin: 20px 0;
    text-align: left;
    color: #000;
    font-weight: 600;
    font-family: 'open sans';
    font-size: 24px;
}

h3 {
    text-align: left;
    margin: 30px 0 10px;
    font-weight:300;
    font-size:22px;
}

.reportDiv {
    /*background-color:#F2F8FA;*/
    margin: 20px 0;
}


/*.traitsDescription p {
    font-size:18px;
}*/

.cell .firstLine {
    font-family: 'open sans condensed';
    display: block;
    font-size: 34px;
    font-weight: 700;
    line-height: 34px;
    text-align: center;
    margin-top: 42px;
}

.cell .secondLine {
    font-family: 'open sans condensed';
    display: block;
    font-size: 22px;
    font-weight: 400;
    line-height: 22px;
    text-align: center;
}

.secondLineInversed {
    font-family: 'open sans condensed';
    display: block;
    font-size: 22px;
    font-weight: 400;
    line-height: 22px;
    text-align: center;
    margin-top:40px;
}

.firstLineInversed {
    font-family: 'open sans condensed';
    display: block;
    font-size: 34px;
    font-weight: 700;
    line-height: 34px;
    text-align: center;
}

.cell {
    border: 1px dashed rgb(147, 229, 237);
    height: 130px;
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}

.cell p {
    margin-top: 55px;
}

.reportLink {
    margin-top: 20px;
    background-color: #000;
    color: #fff;
    padding: 5px;
}

.reportLink:hover, .reportLink:focus, .reportLink:active {
    color:#fff;
    text-decoration:none;
    background-color: rgb(18, 157, 183);
} 

.printLink {
    margin-top:10px;
    cursor:pointer;
}


.printLink img {
    height: 30px;
    margin-right: 10px;
}


.career {
    border-bottom: 1px solid rgb(23, 184, 214);
  
}

.career:hover {
    /*background-image: url(/Content/images/stripespattern8.png);*/
    background-repeat:repeat;  
    text-decoration:none;
    color:#000;
}

.traitsDescription {
    opacity:0;
}

