.annotation {
    color: #1C8791;
    font-weight: 600;
    font-size: 24px;
    font-style: italic;
}

.arrowRight {
    position: absolute;
    right: 0;
    top: 50%;
    font-size: 24px;
    margin-top: -12px;
    line-height:normal;
    margin-right:7px;
}

.blueStripesBtn {
    display: inline-block;
    line-height: 50px;
    padding: 0 10px;
    color: #000;
    background-color: transparent;
    border: none;
    height: 50px;
    text-transform: uppercase;
    font-weight: 600;
    font-size: 17px;
    position: relative;
    padding-right: 30px;
    background-image:url(/Content/images/stripespattern8.png);
    margin:10px 0;
}

.blueStripesBtn:hover, .blueStripesBtn:focus, .blueStripesBtn:active {
    background-image:url(/Content/images/stripespattern6.png);
    color:#000;
    text-decoration:none;
}

.careersByTRaitBtn .arrowRight {
    font-size: 17px;
    margin-top: -12px;
}

.reportDiv .extraMargin {
    margin-top:20px;
    height: 30px;
    margin-bottom: 20px;
}

