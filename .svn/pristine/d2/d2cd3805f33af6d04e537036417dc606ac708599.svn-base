@media only screen and (max-width:1200px) {
    .whiteBgPage .container {
        width:95%;
        padding:10px;
    }

    .pageContainer h2 {
        font-size:22px;
    }

    h3 {
        font-size: 21px;
    }
}


@media only screen and (max-width:992px) {
    .interest {
        font-size: 23px;
    }

    .annotation {
        font-size:20px;
    }
}

@media only screen and (max-width:768px) {
    .reportDiv {
        margin:0 5px 25px;
    }

    .reportDiv .extraMargin {
        margin-bottom: 0;
    }

    .pageContainer h2 {
         font-size: 20px;
    }

    .cell .firstLine, .firstLineInversed {
        font-size: 22px;
    }

    .secondLineInversed {
        margin-top:42px;
    }

    .traitsDescription .text-right {
        text-align:left;
    }

    .blueStripesBtn {
        font-size:14px;
    }

    .traitsDescription p {
        font-size: 13px;
    }

    .interest {
        font-size: 20px;
    }

    .printLink {
        display:none !important;
    }

    .reportDiv .text-right {
        text-align:left;
    }

    .annotation {
        font-size:17px;
    }

}

@media only screen and (max-width:500px) {

    #studentReportPage h1 {
        margin-top: 10px;
    }

    .whiteBgPage .container {
        padding: 0 0 30px;
    }

   .container .col-md-9 {
        padding:5px;
    }

    .pageContainer h2 {
        font-size: 16px;
        margin: 10px 0;
    }

    .cell {
        height:100px;
        width:50%;
    }

    .cell .firstLine {
        margin-top:22px;
        font-size:20px;
    }

    .firstLineInversed {
        font-size:20px;
    }

    .cell .secondLine, .secondLineInversed {
        font-size:18px;
    }

    .secondLineInversed {
        margin-top: 22px;
    }

    h3 {
        margin: 15px 0 10px;
        font-size:16px;
    }

    .blueStripesBtn {
         font-size: 12px;
    }

    .annotation {
        font-size:14px;
    }
}


@media print {
    @page {
        margin: 10px 15px;
        size:auto;
    }

    body {
        margin:0;
        padding:0;
    }

    #studentReportPage .container {
        padding:0;
    }

    .navbar {
        display:block;
    }

    .navbar .print {
        display: inline;
    }

    .navbar-wrapper .container {
        border-bottom:1px solid #000;
        padding: 0 20px;
    }

    .navbar .screen {
        display:none;
    }

    .col-lg-9, .col-md-9 {
        width:100%;
    }

    .traitsDescription {
        opacity:1;
    }

    .blueStripesBtn, .orgRow, .reportLink, .arrowRight {
        display:none;
    }

    /*.pageContainer {
        padding: 0;
    }*/

    .whiteBgPage .container {
        background-color: #fff;
        padding: 20px;
    }

    #studentReportPage h1{
        font-size: 27px;
        text-align: center;
        margin: 20px 0;
    }

    .pageContainer h2 {
        margin: 30px 0 15px;
        font-size: 19px;
    }

    .cell {
        height:100px;
        line-height:100px;
    }

    .cell .firstLine {
        font-size: 26px;
        margin-top: 22px;
    }

    .reportDiv {
        margin:0;
        padding:0;
    }

    .reportDiv .extraMargin {
        margin: 0;
        height: 0;
    }

    h3 {
       font-size: 15px;
        margin: 15px 0 5px;
        font-style: italic;
        font-weight: 400;
    }

    .annotation {
        font-size: 19px;
    }

    .traitsDescription {
        page-break-inside:avoid;
    }

    .traitsDescription p {
        font-size: 14px;
    }

    .interest {
        display: block;
        font-family: 'open sans';
        font-size: 16px;
        line-height: initial;
        font-weight: 400;
    }

      a[href]:after {
        content: none !important;
      }

      footer .container {
          display:none;
      }

    .career {
        padding: 5px 0;
        min-height: initial;
    }

    .noPrint {
        display:none !important;
    }
    .noResultsError {
        text-align:left;
        padding: 0;
        font-size: 18px;
        font-family: 'open sans';
    }

    .professionName {
        font-size:16px;
    }

    #back-top {
        display:none;
    }

    .careersReport, .careersNoReport, .careersYesReport {
        -webkit-column-count: 3; /* Chrome, Safari, Opera */
        -moz-column-count: 3; /* Firefox */
        column-count: 3;
        -moz-column-gap: 30px;
        -webkit-column-gap: 20px;
    }

    .career:last-child {
        border-bottom: 1px solid rgb(23, 184, 214);
    }
}