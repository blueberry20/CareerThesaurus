    <script>
        //open test menu on menu navbar icon click
        var clicked = false;
        $('.bt-menu-trigger').click(function () {
            if (clicked == false) {
                // odd clicks
                $('.bt-menu').addClass('bt-menu-open');

                if (isDeviceMobile && !isiPad) {//mobile - hide
                    $('#continue').css({ 'display': 'none', 'position': 'fixed', 'right': '-300px', 'width': '200px', 'background-color': '#38D5A3;' });
                }
                else { //animate continue button only on desktop - hide
                    var right = $('#continue').offset().right;
                    $("#continue").css({ right: right }).animate({ "right": "-300px" }, "fast");
                }

                var overlay = $('.bt-menu').append('<div class="bt-overlay"></div>');
                $('.bt-overlay').click(function () {
                    resetMenu();
                })
                clicked = true;
            } else {
                // even clicks
                resetMenu();
                $('.bt-overlay').remove();

                clicked = false;
            }
        });

        function resetMenu() {
            $('.bt-menu').removeClass('bt-menu-open');
            $('.bt-menu').addClass('bt-menu-close');

            if (!($('#continue').hasClass('disabled'))) {
                //show continue button
                if (isDeviceMobile && !isiPad) {
                    $('#continue').css({ 'display': 'inline-block', 'position': 'relative', 'right': '0', 'width': '100%', 'background-color': 'rgb(66, 212, 170)' });
                }
                else {//mobile - show
                    var right = $('#continue').offset().right;
                    $("#continue").css({ right: right }).animate({ "right": (winWidth / 2) - 130 + 'px' }, "fast");
                }
            }
        }

        var winWidth,
            winHeight,
            currentQuestion,
            nextQuestion = null,
            delay = 2000,
            animateDelay = 500,
            gender,
            imagePath = '@Url.Content("~/Content/images/skilltest/Page_")',
            appserverurl = "@ViewBag.AppServerUrl",
            isDeviceMobile = "@ViewBag.IsMobileDevice".toLowerCase() == 'true' ? true : false,
            isiPad = "@ViewBag.IsiPad".toLowerCase() == 'true' ? true : false,
            hint = 'Please select a person in the picture that best suits your personality',
            answered = 0,
            dimensions = ["attitude", "endurance", "action", "concentration", "information", "processing", "presence", "patterns", "compensation"],
            images = [],
            showedBefore = false,
            changeAnswer = false,
            name = '@ViewBag.Name',
            user = '@ViewBag.Email',
            profileType = '@ViewBag.ProfileType';

        //---------------------------------------------- document ready function ----------------------------------
        $(document).ready(function () {

            if (isiPad) {
                imagePath = '@Url.Content("~/Content/images/skilltestIpad/Page_")'
            }
            if (isDeviceMobile && !isiPad) {
                $('.blueBorderBtn').css({ 'margin': '10px 0', 'width': '100%' }).find('label').css('color', 'black');
            }

            $('.blueBorderBtn').click(function () {
                $('.blueBorderBtn').removeClass('selectedGender');
                $(this).addClass('selectedGender');
            });


            var nua = navigator.userAgent;
            var isAndroid = (nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 && nua.indexOf('AppleWebKit') > -1 && nua.indexOf('Chrome') === -1);
            if (isAndroid) {
                var pxratio = window.devicePixelRatio;
                $('meta[name=viewport]').attr('content', 'user-scalable=0, width=' + screen.width / pxratio + ', height=' + screen.height / pxratio + ', initial-scale=1.0, maximum-scale=1.0');
            }
            //if (!isDeviceMobile || isiPad) {
            //    $(".mobileMenu").niceScroll({ cursoropacitymin: 0.9, zindex: 10000, cursorwidth: "3px", cursorborder: "1px solid #fff" });
            //}

            function viewport() {
                var e = window, a = 'inner';
                if (!('innerWidth' in window)) {
                    a = 'client';
                    e = document.documentElement || document.body;
                }
                return { width: e[a + 'Width'], height: e[a + 'Height'] };
            }
            function updateVw() {
                var vw = viewport().width / 100;
                jQuery('html').css({
                    'font-size': vw + 'px'
                });
            }

            jQuery(window).ready(function () {
                updateVw();
            });

            if (window.innerHeight < 768) {
                var updatedHeight = window.innerHeight / 9;
                $('#leftside li').height(updatedHeight).width(updatedHeight).css('margin', '0 5px');
                $('.desktopNav').css({ height: '100%', width: '100%' });
            }

            jQuery(window).resize(function () {
                updateVw();
                if (window.innerHeight < 768) {
                    var updatedHeight = window.innerHeight / 9;
                    $('#leftside li').height(updatedHeight).width(updatedHeight).css('margin', '0 5px');
                    $('.desktopNav').css({ height: '100%', width: '100%' });
                }
            });

            if ("@ViewBag.skilltestModal") {  //==================  Modal view  =====================
                //document.addEventListener('touchstart', function (event) {
                //    if (!isTextInput(event.target) && isTextInput(document.activeElement)) {
                //        document.activeElement.blur();
                //    } else if (isBtn) {
                //        checkFields();
                //    }
                //}, false);
                //tempnewtest();
                function isTextInput(node) {
                    return ['INPUT'].indexOf(node.nodeName) !== -1;
                }

                function isBtn(node) {
                    return ['nextBtn'].indexOf(node) !== -1;
                }

                $('input').keyup(function (event) {
                    event = (event) ? event : window.event;
                    var charCode = (event.which) ? event.which : event.keyCode;
                    if (charCode == 13) {
                        document.activeElement.blur();
                    }
                });

                if (isDeviceMobile && !isiPad) {

                    // ================================mobile modal================================
                    //$('.questionContent').css('height', ((pxratio) ? window.innerHeight / pxratio : window.innerHeight) - 135);
                    var slidesHeight = (pxratio ? window.innerHeight / pxratio : window.innerHeight) - 125;
                    $('#slides').css('height', slidesHeight > 400 ? slidesHeight : 400);
                    $('.jobs').css('height', '100%');
                }
                else if (isDeviceMobile && isiPad) {
                    $('.imageDiv').append('<img id="background" class="img" src="/Content/images/skilltest/Page_1/female_bg.jpg" />');
                    //$('#skillTestModal').modal('show');
                    $('#tempModal').modal('show');
                    $('.jobs').css('overflow-y', 'scroll');
                    // $('.jobs .job').css({ padding: '8px 10px', 'border-bottom': '1px solid rgb(228, 248, 252)'});
                }
                else {
                    $('.imageDiv').append('<img id="background" class="img" src="/Content/images/skilltest/Page_1/female_bg.jpg" />');
                    $('#tempModal').modal('show');
                    //$('#skillTestModal').modal('show');
                    $('.jobs').niceScroll({ cursoropacitymin: 0.9, zindex: 10000, cursorwidth: "3px", cursorborder: "0", autohidemode: false, cursorcolor: "#D9DADB" });
                }
                window.onorientationchange = function (event) {
                    var slidesHeight = (pxratio ? window.innerHeight / pxratio : window.innerHeight) - 125;
                    $('#slides').css('height', slidesHeight > 400 ? slidesHeight : 400);
                }
                showJobsSelection('');
                displaySize();
                //  modalPosition();


                $(window).resize(function () {
                    displaySize();
                    // modalPosition();
                });

                $('#titleSelect').change(function () {
                    getTitle();
                });

                //step 1.
                //generate years for grad year select
                var yearslisted;
                var myDate = new Date();
                var year = myDate.getFullYear();
                for (var i = year; i > year - 100; i--) {
                    yearslisted += '<option value="' + i + '">' + i + '</option>'
                }
                $('#gradYearSelect2').append(yearslisted);


                //step 2 slide 1
                $('#yesWorkBtn').click(function () {
                    $('#slide2').stop().animate({ left: '0' }, 500, 'easeOutBack');
                    $('#slide1 .fa-angle-right').show();
                    $('#slide2 .icon-left-open-big').show();
                })

                $('#noWorkBtn').click(function () {
                    //$('#collapseTwo').collapse('toggle');
                    //$('#collapseThree').collapse('toggle');
                    $('#collapseThree').removeClass('collapse').addClass('in');
                    $('#collapseTwo').removeClass('in').addClass('collapse');
                })

                //step 2 slide 2
                $('#satisfiedYesBtn').click(function () {
                    $('#slide3').stop().animate({ left: '0' }, 500, 'easeOutBack');
                    $('#slide2 .fa-angle-right').show();
                    $('#slide3 .icon-left-open-big').show();
                    // $('#slide2 .slideContent').hide();
                })


                //step 2. arrows to switch between slides.
                $('#skillTestModal .icon-left-open-big').click(function () {
                    $(this).closest('.slide').animate({ left: '100%' }, 500, 'easeOutBack');
                })

                $('#skillTestModal .fa-angle-right').click(function () {
                    $(this).closest('.slide').next().animate({ left: '0%' }, 500, 'easeOutBack');
                })

                //step 2 slide 3

                $('.jobs').click(function (event) {
                    var target = $(event.target);
                    if (!target.is('div')) {
                        pickThisJob(target.parent());
                    } else {
                        pickThisJob(target);
                    }
                    setTimeout(function () {
                        $('#professionsOverlay').hide();
                        if (isDeviceMobile && !isiPad) {
                            $('#skillTestModal').show();
                        }
                    }, 500);

                });

                $('.startNewTest').click(startNewTest);
                $('.tempNewTest').click(tempnewtest);

                $('.closeModal').click(function () {

                    if (document.referrer && document.referrer.toLowerCase().indexOf('skilltest') == -1 && document.referrer && document.referrer.toLowerCase().indexOf('careertest') == -1) {
                        window.history.back();
                    } else {
                        window.location = "/home";
                    }

                });
                if (ga) ga('send', 'event', 'page', 'Question number', 'pre test');
            } else { //================================  Test View ================================

                currentQuestion = parseInt('@ViewBag.id');
                if (ga) ga('send', 'event', 'page', 'Question number', currentQuestion);
                gender = '@ViewBag.gender';

                window.onorientationchange = function (event) {
                    if (isAndroid) {
                        location.reload();
                    }
                    if (isiPad) {
                        displaySize();
                        resizeMapping(currentQuestion);
                        var positionXY = tooltipMapping($('.selectedPerson').data('positionx'), $('.selectedPerson').data('positiony'));
                        $('.selected-tooltip').css({ left: positionXY[0] - ($('.selected-tooltip').innerWidth() / 2), top: positionXY[1] - ($('.selected-tooltip').innerHeight()) });
                        continueBtnPosition();
                    }
                }

                constractor(currentQuestion);

                $('map, area').click(function (event) {
                    event.preventDefault();
                });

                document.ondragstart = function () {
                    return false;
                }


                //open/close nav menu overlay in mobile
                $('#testMenuIcon').click(function () {
                    $('.mobileMenu').show();
                    $('.mobileWrapper').hide();
                    $('.headersText').hide();
                    $('html, body').css('height', '100%');

                })

                $('#testCloseIcon').click(function () {
                    $('.mobileMenu').hide();
                    $('.mobileWrapper').show();
                    $('.headersText').show();
                });

                $(window).resize(function () {
                    if (!isDeviceMobile || isiPad) {
                        displaySize();
                        resizeMapping(currentQuestion);
                        var positionXY = tooltipMapping($('.selectedPerson').data('positionx'), $('.selectedPerson').data('positiony'));
                        $('.selected-tooltip').css({ left: positionXY[0] - ($('.selected-tooltip').innerWidth() / 2), top: positionXY[1] - ($('.selected-tooltip').innerHeight()) });
                        continueBtnPosition();
                    }
                    displaySize();
                });
                // window.addEventListener("orientationchange", $(window).trigger('resize'));
                $('.desktopNav, .mobileNavigation').click(function () {
                    var questionNumber = this.id.slice(-1);
                    $('#moveToQuestion').val(questionNumber);
                    //$('#navigationForm').attr('action', 'SkillTest/' + questionNumber);
                    $('#navigationForm').submit();
                });

                $('.desktop button').on('mouseenter', function () {
                    var questionNumber = this.id.slice(-1);
                    preloadImages(parseInt(questionNumber) - 1);
                });

                $('.startOver').click(function (event) {
                    for (var i = 0; i < 9; i++) {
                        setCookie(dimensions[i], '', -1);
                    }
                    answers = 0;
                    setCookie('question1', '', -1);
                    setCookie('gender', '', -1);
                    setCookie('firstname', '', -1);
                    setCookie('lastname', '', -1);
                    setCookie('email', '', -1);
                    setCookie('clr', '', -1);
                });

                $('#continue').click(function (event) {
                    if (!$(this).attr('href')) {
                        event.preventDefault();
                        $('#moveToQuestion').val(nextQuestion);
                        $('#navigationForm').submit();
                    } else {
                        if (ga) ga('send', 'event', 'page', 'Question number', 'Results');
                    }
                });

                $('.slideOut').click(function () {
                    if ($('#navigationWrapper').hasClass('visible')) {
                        $('#navigationWrapper').animate({ bottom: '-60px' }, 500).removeClass('visible');
                    } else {
                        $('#navigationWrapper').animate({ bottom: '-10px' }, 500).addClass('visible');
                    }
                });
            }

            changeAds();
            $(window).resize(function () {
                changeAds();
            })

        }); // END document ready

        function changeAds() {
            if ($(window).width() < 992) {
                $('#leftBanner120').empty();
                $('#rightBanner120').empty();
                if ($('#bottomBanner').is(':empty')) {
                    $('#bottomBanner').append('<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"><\/script><ins class="adsbygoogle" style="display:block" data-ad-client="ca-pub-3486420009474810" data-ad-slot="6576327288" data-ad-format="auto"></ins><script>(adsbygoogle = window.adsbygoogle || []).push({});<\/script>');
                }
            }
            else {
                $('#bottomBanner').empty();
                if ($('#leftBanner120').is(':empty')) {
                    $('#leftBanner120').append('<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"><\/script><ins class="adsbygoogle"style="display:inline-block;width:120px;height:600px"data-ad-client="ca-pub-3486420009474810" data-ad-slot="3491211286"></ins><script>(adsbygoogle = window.adsbygoogle || []).push({});<\/script>');
                }

                if ($('#rightBanner120').is(':empty')) {
                    $('#rightBanner120').append('<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"><\/script><ins class="adsbygoogle"style="display:inline-block;width:120px;height:600px"data-ad-client="ca-pub-3486420009474810" data-ad-slot="3491211286"></ins><script>(adsbygoogle = window.adsbygoogle || []).push({});<\/script>');
                }
            }
        }
    </script>

