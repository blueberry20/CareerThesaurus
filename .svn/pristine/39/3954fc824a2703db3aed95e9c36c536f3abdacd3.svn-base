                        AssessmentCareer rating = acrc.GetByPartitionAndRowKey(student.School, email + student.Year + student.Grade + ViewBag.Dolcode);
                        string like = "";
                        string dislike = "";
                        if (rating != null)
                        {
                            if (rating.Value == "0")
