.pageContainer {
    padding-top: 50px;
    padding-bottom: 0;
    min-height: 100%;
    /*background-image: url(/Content/images/hex/hex_bg14.jpg);
    background-attachment:fixed;*/
}

.pageContainer .container {
    background-color: #fff;
    padding: 20px 15px;
    position:relative;
}

.header {
    font-size: 38px;
    font-weight: 700;
    text-align: center;
    padding-bottom:15px;
    font-family: 'open sans condensed';
    text-transform: uppercase;
}

#changeAnswersBtn {
    color: #000;
    width: initial;
    min-width: initial;
    font-size: 14px;
    margin: 0;
    text-decoration: none;
    line-height: 50px;
    padding: 0 10px;
    text-align: center;
    background-color: transparent;
    border: 1px solid #27AEC0;
    /*position: absolute;
    left: 20px;*/
}

#changeAnswersBtn .arrowLeft {
    margin-top: 14px;
    font-size: 20px;
}

#changeAnswersBtn:hover, #changeAnswersBtn:focus, #changeAnswersBtn:active {
    background-color: rgba(23, 184, 214, 1);
}

.subheader {
    font-size: 18px;
    color: #333;
    padding: 8px;
    margin-bottom:10px;
    text-align:center;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

.proTitle {
    display: block;
    font-size: 24px;
    font-weight:600;
    padding-right: 120px;
}

.profession {
    padding: 10px;
    border-bottom: 1px solid rgb(23, 184, 214);
    display: block;
    position:relative;
}

.subfooter {
    min-height:50px;
    text-align:right;
    display:table;
    width:100%;
}

.subfooter a {
    font-size: 15px;
    min-height: 40px;
    padding: 0 5px;
    color: #000;
    color: #0E83CA;
    text-align: right;
    position:relative;
    display: table-cell;
    width: 100%;
    vertical-align: middle;
}

.subfooter .fa-angle-right {
    font-size: 19px;
    position: absolute;
    right: 10px;
    top: 50%;
    margin-top: -9px;
}

.linkText {
    padding-right: 25px;
    display:block;
}

.linkText1 {
    padding-right: 25px;
    border: 1px solid #5BACDE;
    padding: 7px 25px 7px 7px;
    border-radius: 20px;
    text-transform: uppercase;
    font-size: 14px;
    display: inline-block;
}

.indeedCount {
    display: block;
    font-size: 13px;
    min-height: 17px;
    margin-left:16px;
    padding-right: 110px;
}

.blueStripesBtn {
    float: right;
    color:#000;
    margin:0;
    text-decoration:none;
    line-height:50px;
    padding: 0 10px;
    text-align: center;
    padding-right:50px;
    /*background-image: url(/Content/images/stripespattern6.png);*/
}
  
.btn-success {
    float:right;
}

.blueStripesBtn:hover, .blueStripesBtn:focus, .blueStripesBtn:active {
    /*background-image: url(/Content/images/stripespattern8.png);*/
    text-decoration:none;
    color: #000;
}

.btn-success .badge {
    background-color: #EF3333 !important;
    color: #fff !important;
    position: absolute;
    top: -9px;
    right: -10px;
    padding: 4px 7px;
    box-shadow: 1px 1px 4px #666;
}

.viewJobsBtn {
    position:absolute;
    top: 50%;
    right: 0;
    margin-top: -25px;
}

.viewJobs:hover, .viewJobs:focus {
    text-decoration:none;
}
.red {
    color: #e15470;
}
.asTitle {
    background-color: #000;
    color:#fff;
    padding: 0 5px;
    font-family:'open sans condensed';
}


.btn-blue {
    background-color: #fff;
    border: 1px solid #27AEC0;
}



.btn-group-justified>.btn-group .btn {
    width: 100%;
    height: 70px;
    background-color: #BBF3F8;
    border: 1px solid #fff;
    font-size: 20px;
    font-weight: 300;
}

.btn-blue.active {
   background-color: rgb(99, 220, 243) !important;
}

.btn-blue.active:hover, .btn-blue.active:focus, .btn-blue.active:active {
    background-color: rgb(99, 220, 243) !important;
}

.btn-blue:hover, .btn-blue:focus, .btn-blue:active {
   background-color: rgb(99, 220, 243) !important;
}

.btn-group-justified {
    padding-bottom:20px;
}


.fieldWrapper {
    opacity: 0;
    filter: alpha(opacity=0);
    margin-bottom: 40px;
    background-color: #F9FDFE;
      -webkit-transition: all .55s ease-in;
  -moz-transition: all .55s ease-in;
  -ms-transition: all .55s ease-in;
  -o-transition: all .55s ease-in;
  transition: all .55s ease-in;
  position:relative;
}

.fieldWrapper.viewed {
    opacity:1;
    filter: alpha(opacity=100);
}

@media only screen and (max-width:992px) {

    .pageContainer .container {
        width: 90%;
    }

    .header {
        font-size: 30px;
    }

    .proTitle {
        font-size: 20px;
    }

    .indeedCount {
        font-size: 12px;
    }

    .subfooter a {
        font-size: 13px;
        min-height:40px;
        margin-top:0;
    }

    .subfooter {
        min-height:40px;
    }

    .fieldWrapper {
        margin-bottom: 20px;
    }

    .subheader {
        font-size:18px;
    }

    #changeAnswersBtn {
        font-size:12px;
    }
}

@media only screen and (max-width:767px) {

    .pageContainer .container {
        width: 100%;
        padding:10px;
    }

    .btn-group-justified {
        padding-bottom: 15px;
    }

    .btn-group-justified>.btn-group .btn {
        height: 50px;
        font-size: 15px;
    }

    .header {
        font-size: 25px;
    }

    .fieldWrapper {
        margin-bottom: 10px;
    }

    .viewJobsBtn {
        height: 40px;
        line-height: 40px;
        margin-top: -20px;
    }

    .proTitle {
        font-size: 18px;
    }

    .subheader {
        font-size: 16px;
    }

    .subfooter a {
        text-align:left;
    }

    #changeAnswersBtn {
        font-size: 12px;
        height: 40px;
        line-height: 40px;
    }

    #changeAnswersBtn .arrowLeft {
        margin-top: 11px;
        font-size: 17px;
    }
}

@media only screen and (max-width:500px) {
    .btn-group-justified>.btn-group .btn {
        min-width:initial;
        font-size: 12px;
        font-weight:400;
    }

    .header {
        font-size: 20px;
        padding-top: 10px;
    }

    .subheader {
        font-size: 13px;
    }

    .viewJobsBtn {
        font-size: 12px;
        min-width: 100px;
        width:100px;
    }

    .btn-success .badge {
        font-size: 9px;
    }

    .proTitle {
        font-size: 15px;
    }

    .indeedCount {
        font-size: 10px;
        margin-left: 6px;
    }

    .subfooter a {
        font-size: 11px;
        min-height:initial;
    }

    #changeAnswersBtn {
        width:100%;
    }
}
