.pageContainer.lockedMode {
    background-image: url(/Content/images/hex/graybackground2.jpg);
}

.lockedMode .container {
    background-color: #fff;
    padding: 0 15px 50px;
}

h1 {
    text-align:center;
    margin:4px 0;
    padding: 20px;
    font-family:'open sans condensed';
    font-weight:700;
    font-size:38px;
}

.panel {
    background-color:#fff;
}

.sideNav {
    display:block;
    background-color: rgb(229, 245, 250);
    text-align:center;
    height:50px;
    line-height:50px;
    color: #000;
    text-transform:uppercase;
    margin-bottom:5px;
    padding:0 10px;
    font-weight:600;
    position:relative;
}

.sideNav:hover, .sideNav:focus, .sideNav:active,.activeTab {
    background-color: rgb(23, 184, 214);
    text-decoration:none;
    color: #000;
}

.sideNav .fa-angle-right {
    position:absolute;
    right:15px;
    top:10px;
    top: 15px;
    font-size: 20px;
}

.school-wrapper {
    background-color: #fff;
    padding: 10px;
    margin: 0 0 4px;
    /*border-bottom: 1px solid rgb(23, 184, 214);*/
    display:table;
    width: 100%;
    background-color: rgb(247, 251, 252);
}

.school-wrapper:hover, .school-wrapper:focus, .school-wrapper:active {
    -moz-box-shadow: 0 0 1px #888;
    -webkit-box-shadow: 0 0 1px#888;
    box-shadow: 0 0 1px #888;
}

.imageWrapper {
    display: table-cell;
    width: 20%;
    vertical-align: middle;
    padding:5px
}

.textWrapper {
    display: table-cell;
    width: 80%;
    padding:5px;
}

.leftSide {
    padding: 0 2px 0 0;
}

.rightSide {
    padding: 0 0 0 2px;
}

.school-wrapper a {
    text-decoration: none;
    color:#000;
}

.school-wrapper a:hover, .school-wrapper a:focus, .school-wrapper:active {
    text-decoration: none;
    color:#000;
}

.school-wrapper p {
    margin-bottom:4px;
}

.headline {
    font-weight:600;
    margin: 10px 0;
    font-size: 18px;
}

.divider {
    width:200px;
    margin: 0  auto;
    background-color:rgb(23, 184, 214);
    height:1px;
    margin:10px auto;
}

.ad {
    margin:25px 0;
}

.rightColumn {
    padding:0;
}

@media screen and (max-width:992px) {
    .pageContainer .container {
        width: 100%;
    }

    h1 {
        font-size:30px;
    }

    .sideNav {
        font-size:10px;
        padding:0 5px;
    }

    .textWrapper p {
        margin-bottom:5px;
    }

    .headline {
        font-size:16px;
    }

    .sideNav .fa-angle-right {
        right: 5px !important;
        top: 18px !important;
        font-size: 16px !important;
    }
}

@media screen and (max-width:768px) {
    .sideNav {
        font-size: 11px;
        width: 33.3%;
        margin: 0;
        border: 1px solid #fff;
        padding:0 5px;
        line-height: initial;
        display: table-cell;
        vertical-align: middle;
   }

    .leftSide, .rightColumn {
        width:100%;
        padding:0;
    }

    .rightSide {
        display:table;
        width:100%;
        margin: 0 !important;
        padding:0;
    }

    .lockedMode .container {
        padding:5px;
    }

    h1 {
        padding:10px;
    }

    .school-wrapper {
        padding:10px;
    }

    .textWrapper p {
        font-size:13px;
    }

    .textWrapper {
        padding:5px;
    }
}

@media screen and (max-width:600px) {
    .imageWrapper {
        width:100%;
        display:block;
    }

    .textWrapper {
        width: 100%;
        display:block;
    }

    .school-wrapper {
        background-color: #fff;
        border-bottom: 1px solid rgb(23, 184, 214);
    }
}

@media screen and (max-width:500px) {
    h1 {
        font-size:22px;
    }

    .sideNav {
        font-size:10px;
    }

    .sideNav .fa-angle-right {
        font-size:16px;
        top:18px;
    }

    .imageWrapper {
        text-align:center;
    }

    .headline {
        font-size:14px;
    }

    .textWrapper p {
        margin-bottom: 5px;
        font-size: 13px;
    }
}