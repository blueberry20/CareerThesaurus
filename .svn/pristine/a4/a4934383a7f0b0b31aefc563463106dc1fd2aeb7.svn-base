/*==============Step 1==================*/


#traitsDiv {
    display:block;
}

#dimensionsTable {
    width:100%;
    border-spacing: 0 8px;
    border-collapse: separate;
}

#dimensionsTable:before, #dimensionsTable:after {
    display:none;
} 

.textWrapper {
    /*height:66px;*/
    padding:4px 0 10px;
    text-align:center;
}

.tableRow {
    display:table-row;
}
.dimension {
    padding:0;
    height:66px;
}

.dimension::before, .dimension::after {
    display:none;
}

.dimensionCell {
    border:1px solid #4ABADB;
    height: 60px;
    text-align: center;
    vertical-align: middle;
    font-size: 20px;
    color: #000;
    font-weight: 600;    
    cursor:pointer;
    width:48%;
    font-family:inherit;
    padding:8px 4px;
    margin:0 2px 4px;
}

.dimensionCell.active {
    background-color: rgba(143, 214, 228, 1);
    box-shadow: 3px 3px 5px #949292;
    -moz-transition: background 0.5s linear;
    -o-transition: background 0.5s linear;
    -webkit-transition: background 0.5s linear; /* Chrome, Safari, Opera */
    transition: background 0.5s linear;
    color: #000;
    width:56%;
    height:60px;
    padding:8px 4px;
    margin:0 2px 4px;
}

.notActive {
    width: 40%;
    height: 50px;
    padding: 3px 4px;
    margin: 5px 2px;
}

.dimensionCell.active:hover, .dimensionCell.active:focus, .dimensionCell.active:active {
    color:#000;
}


  .dimensionCell:hover, .dimensionCell:active, .dimensionCell:focus {
    color:rgb(23, 184, 214);
    color:rgba(23, 184, 214, 1);
}


/*odd rows*/
#dimensionsTable .dimension:nth-child(odd) .firstLine, #dimensionsTable .dimension:nth-child(odd) .secondLine {
    font-family:'open sans condensed';
    display: block;
    font-size: 18px;
    font-weight: 400;
    line-height:22px;
}

/*even rows*/
#dimensionsTable .dimension:nth-child(even) .firstLine, #dimensionsTable .dimension:nth-child(even) .secondLine {
    font-family:'open sans';
    display: block;
    font-size: 14px;
    line-height:22px;
    font-weight: 400;
}

/*bigger font in odd row- condensed font*/
#dimensionsTable .dimension:nth-child(odd) .firstLine {
    font-size: 20px;
}

/*bigger font in even row*/
#dimensionsTable .dimension:nth-child(even) .firstLine {
    font-size:16px;
}

/*bigger font when active- odd*/
#dimensionsTable .dimension:nth-child(odd) .dimensionCell.active .firstLine {
    font-size:26px;
}

/*bigger font when active - even*/
#dimensionsTable .dimension:nth-child(even) .dimensionCell.active .firstLine {
    font-size:22px;
}

#dimensionsTable .dimension .firstLine {
    font-weight:700 !important;
}



