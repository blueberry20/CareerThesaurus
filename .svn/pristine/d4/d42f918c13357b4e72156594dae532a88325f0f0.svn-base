<body>
    <form id='navigationForm' method="post">
        <input type="hidden" id="moveToQuestion" name="questionNumber" value="" />
    </form>
    @if (@ViewBag.skilltestModal != null)
    {

        if (!@ViewBag.IsMobileDevice || @ViewBag.IsiPad)
        {
            <!--===================  Modal Start Test. Desktop Version ===================-->
            <div class="imageDiv">
            </div>

            @* temp modal 1 question about gender *@
            <div class="modal fade desktop" id="tempModal" aria-hidden="true" data-backdrop="static" data-keyboard="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">Select your gender</h4>
                        </div>
                        <div class="modal-body">
                            <div class="radio blueBorderBtn">
                                <label>
                                    <input type="radio" name="gender" id="male" value="male">
                                    Male
                                </label>
                            </div>
                            <div class="radio blueBorderBtn">
                                <label>
                                    <input type="radio" name="gender" id="female" value="female">
                                    Female
                                </label>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default closeModal" data-dismiss="modal">Close</button>
                            <button type="button" class="btn tempNewTest">Start</button>
                        </div>
                    </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
            </div><!-- /.modal -->



            <div class="modal fade desktop" id="skillTestModal" tabindex="-1" role="dialog" aria-labelledby="skilltestModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
                <div class="modal-dialog modal-dialog-center">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title text-center" id="skilltestModalLabel">Career Assessment Test</h4>
                        </div>
                        <div class="modal-body text-center">
                            <div class="panel-group" id="accordion">
                                <!--*********************Step 1************************-->
                                <div class="panel panel-default">
                                    <div id="headingOne" class="panel-heading">
                                        <div data-parent="#accordion" data-target="#collapseOne">
                                            Step 1
                                        </div>
                                    </div>

                                    <div id="collapseOne" class="panel-collapse collapse in">
                                        <div class="panel-body">
                                            <div class="panelContent">
                                                <div class="row">
                                                    @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                            <p>Title</p>
                                                        </div>*@
                                                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                        <select id="titleSelect" class="form-control">
                                                            <option selected disabled value="none">Title</option>
                                                            <option value="ms">Ms.</option>
                                                            <option value="mrs">Mrs.</option>
                                                            <option value="mr">Mr.</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                            <p>First name</p>
                                                        </div>*@
                                                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                        <input type="text" class="form-control" id='firstName' name="firstName" placeholder="First Name" autofocus>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                            <p>Last name</p>
                                                        </div>*@
                                                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                        <input type="text" class="form-control" id='lastName' name="lastName" placeholder="Last Name" autofocus>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                            <p>Your email</p>
                                                        </div>*@
                                                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                        <input type="email" class="form-control" id='email' name="Email" placeholder="Email">
                                                    </div>
                                                </div>

                                                <div class="row lastRow">
                                                    <p class="errorMsg"></p>
                                                </div>
                                                <button class="btn nextBtn" onclick="checkFields()">Next</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--*********************Step 2************************-->
                                <div class="panel panel-default">
                                    <div id="headingTwo" class="panel-heading">
                                        <div id="collapseTitle2" data-parent="#accordion" data-target="#collapseTwo">
                                            Step 2
                                        </div>
                                    </div>

                                    <div id="collapseTwo" class="panel-collapse collapse">
                                        <div class="panel-body">
                                            <div id="slides">
                                                <div id="slide1" class="slide">
                                                    <div class="slideContent">
                                                        <p>Are you currently working?</p>
                                                        <div class="btn-group btn-group-justified" id="worknoToggle">
                                                            @* data-toggle="buttons">*@
                                                        <button type="button" class="btn preTestBtn" id="yesWorkBtn">Yes</button>
                                                        <button type="button" class="btn preTestBtn" id="noWorkBtn" onclick="movetoslidethree()">No</button>
                                                        @*<label class="btn preTestBtn">
                                                                <input type="radio" name="options" id="yesWorkBtn">Yes
                                                            </label>
                                                            <label class="btn preTestBtn" onclick="movetoslidethree()">
                                                                <input type="radio" name="options" id="noWorkBtn">No
                                                            </label>*@
                                                    </div>
                                                </div>
                                                <button class="btn pull-right skipBtn" onclick="movetoslidethree()">Skip Survey</button>
                                                <i class="icon-right-open-big"></i>
                                            </div>

                                            <div id="slide2" class="slide">
                                                <div class="slideContent">
                                                    <p>Are you satisfied with your job?</p>
                                                    <div class="btn-group btn-group-justified" id="satisfiedToggle">
                                                        @* data-toggle="buttons">*@
                                                    <button type="button" class="btn preTestBtn" id="satisfiedYesBtn">Yes</button>
                                                    <button type="button" class="btn preTestBtn" id="satisfiedNoBtn" onclick="movetoslidethree()">No</button>
                                                    @*<label class="btn preTestBtn">
                                                            <input type="radio" name="options" id="satisfiedYesBtn">Yes
                                                        </label>
                                                        <label class="btn preTestBtn" onclick="movetoslidethree()">
                                                            <input type="radio" name="options" id="satisfiedNoBtn">No
                                                        </label>*@
                                                </div>
                                            </div>
                                            <button class="btn pull-right skipBtn" onclick="movetoslidethree()">Skip Survey</button>
                                            <i class="icon-left-open-big"></i>
                                            <i class="icon-right-open-big"></i>
                                        </div>

                                        <div id="slide3" class="slide">
                                            <div class="slideContent">
                                                <div class="row">
                                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 text-right">
                                                        <p>Your profession</p>
                                                    </div>
                                                    <div class="col-lg-9 col-md-9 col-sm-9">
                                                        <input id="searchJob" type="text" class="form-control" onkeyup="searchJob(this.value)" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-3"></div>
                                                    <div class="col-lg-9">
                                                        <div class="jobs" id="professionList">

                                                        </div>
                                                    </div>
                                                </div>
                                                @*<div class="row">
                                                        <div class="col-lg-3 text-right"><p class="other">Other career:</p></div>
                                                        <div class="col-lg-9">
                                                            <input id="otherJob" type="text" class="form-control" placeholder="Enter your job title" onkeyup="otherJob(this.value)" />
                                                        </div>
                                                    </div>*@
                                                <div class="row lastRow">
                                                    <button class="btn pull-right nextBtn" onclick="movetoslidethree()">Next</button>
                                                    <button class="btn pull-right skipBtn" onclick="movetoslidethree()">Skip Survey</button>
                                                </div>
                                            </div>
                                            <i class="icon-left-open-big"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--*********************Step 3************************-->
                        <div class="panel panel-default">
                            <div id="headingThree" class="panel-heading">
                                <div id="collapseTitle3" data-parent="#accordion" data-target="#collapseThree">
                                    Step 3
                                </div>
                            </div>
                            <div id="collapseThree" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <div class="panelContent">
                                        <p class="text-center">For better results please provide</p>
                                        <div class="row">
                                            @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                    <p>Zip Code</p>
                                                </div>*@
                                            <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                <input type="text" class="form-control" id='zipCode' name="zipcode" onpaste="return false;" onkeypress="return isNumber(event, this.value);" placeholder="Your Zip Code" autofocus>
                                            </div>
                                        </div>

                                        <div class="row">
                                            @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                    <p>Graduation Year</p>
                                                </div>*@
                                            <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                <select id="gradYearSelect" class="form-control">
                                                    <option selected disabled value="none">Your Graduation Year</option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="row">
                                            @*<div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                                    <p>Education Level</p>
                                                </div>*@
                                            <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-12 col-xs-12">
                                                <select id="educationLevelSelect" class="form-control">
                                                    <option selected disabled value="none">Your Education Level</option>
                                                    <option value="Still in High School">Still in High School</option>
                                                    <option value="GED">GED</option>
                                                    <option value="High School Diploma">High School Diploma</option>
                                                    <option value="Certificate">Certificate</option>
                                                    <option value="Some College">Some College</option>
                                                    <option value="Associates Degree">Associates Degree</option>
                                                    <option value="Bachelor's Degree">Bachelor's Degree</option>
                                                    <option value="Master's Degree'">Master's Degree</option>
                                                    <option value="Doctoral Degree">Doctoral Degree</option>
                                                    <option value="None">None</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="row text-center">
                                            <button type="button" class="startNewTest btn">Begin test</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="closeModal btn btn-default" data-dismiss="modal">Back</button>
                    @*<a href="/Home" id="homeButton" class="btn iconWrapper"><img class="icon" src='@Url.Content("~/Content/images/home.png")' alt="home icon" /><p class="p1">Home</p></a>*@
                    <button type="button" class="startNewTest btn disabled">Begin Test</button>
                </div>
            </div>
        </div>
    </div>
        }
        else
        {
            <!--*************************MOBILE PRE TEST QUESTIONS******************************-->
