#byInterestPage {
    padding:50px 0 0;
}

#byInterestPage h2 {
    margin: 20px 20px 40px;
    text-align: center;
}

#byInterestPage .container {
    padding:0;
}

#main {
    padding: 20px 20px 0;
}

.imgOverlay {
    position:relative;
    overflow:hidden;
}

.textOverlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color:rgba(0, 36, 29, 0.6);
    cursor:pointer;
}

.textOverlay p {
    margin: 10px;
    color: #fff;
    font-size: 24px;
    text-transform: uppercase;
    font-weight: 300;
    text-align: center;
    position: absolute;
    top: 50%;
    margin: 0;
    width:100%;
    padding:0 10px;
}

.interestWrapper {
    padding-bottom:30px;
}

.checkmark {
    opacity:0;
    -webkit-transition: all 0.5s ease-in;
    -moz-transition: all 0.5s ease-in;
    -ms-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
    position: absolute;
    left: 50%;
    margin-left: -31px;
    bottom: 10px;
}

/*.imgOverlay.selected .checkmark {
    opacity:1;
}*/

.imgOverlay.selected .textOverlay {
    background-color:rgba(9, 193, 201, 1.0);
}

.no-touch .imgOverlay:hover .textOverlay, .no-touch .imgOverlay:focus .textOverlay, .no-touch .imgOverlay:active .textOverlay {
    background-color:rgba(9, 193, 201, 0.9);
}

.no-touch .imgOverlay:hover .img-responsive {
    -ms-transform: scale(1.2,1.2); /* IE 9 */
    -webkit-transform: scale(1.2,1.2); /* Chrome, Safari, Opera */
    -o-transform: scale(1.2, 1.2);
    -moz-transform: scale(1.2, 1.2);
    transform: scale(1.2,1.2);
        -webkit-transition: all 0.3s ease-in;
    -moz-transition: all 0.3s ease-in;
    -ms-transition: all 0.3s ease-in;
    -o-transition: all 0.3s ease-in;
    transition: all 0.3s ease-in;
}

.imgOverlay.selected .checkmark {
  opacity:1;
  animation: bounce linear 0.5s;
  animation-iteration-count: 1;
  /*transform-origin: 50% 50%;*/
  -webkit-animation: bounce linear 0.5s;
  -webkit-animation-iteration-count: 1;
  /*-webkit-transform-origin: 50% 50%;*/
  -moz-animation: bounce linear 0.5s;
  -moz-animation-iteration-count: 1;
  /*-moz-transform-origin: 50% 50%;*/
  -o-animation: bounce linear 0.5s;
  -o-animation-iteration-count: 1;
  /*-o-transform-origin: 50% 50%;*/
  -ms-animation: bounce linear 0.5s;
  -ms-animation-iteration-count: 1;
  /*-ms-transform-origin: 50% 50%;*/
}

#selectionSummary {
    background-color: rgb(143, 214, 228);
    padding: 30px 20px;
    margin: 0 -20px;
    font-size: 24px;
    text-align: center;
    font-weight:300;
}

.tryMatchingBtn {
    width:250px;
    display:none;
}

#viewResultsBtn {
    width: 260px;
    height: 70px;
    font-size: 16px;
    line-height: 70px;
    padding-right: 50px;
    padding: 0 50px 0 10px;
    font-weight: 300;
    position:relative;
    margin:20px 0 40px;
}

#viewResultsBtn .fa-angle-right {
    position: absolute;
    right: 13px;
    font-size: 24px;
    top: 50%;
    margin-top: -12px;
    font-weight: 300;
}

.twoButtonsModal .modal-footer {
    background-color: transparent;
    display:table;
    width:100%;
}

#changeInterests {
   background-color: rgb(9, 193, 201);
}

#goToResults {
    background-color: #38D5A3;
}

.modal-footer .oneButton {
    background-color: rgb(9, 193, 201);
}

.modal-footer .oneButton:hover {
    color:#fff;
    background-color: rgb(9, 193, 201);
}

@keyframes bounce {
  0% {
    transform:  translateY(9px)  ;
  }
  15% {
    transform:  translateY(-25px)  ;
  }
  30% {
    transform:  translateY(0px)  ;
  }
  45% {
    transform:  translateY(-15px)  ;
  }
  60% {
    transform:  translateY(0px)  ;
  }
  75% {
    transform:  translateY(-5px)  ;
  }
  100% {
    transform:  translateY(0px)  ;
  }
}

@-moz-keyframes bounce {
  0% {
    -moz-transform:  translateY(9px)  ;
  }
  15% {
    -moz-transform:  translateY(-25px)  ;
  }
  30% {
    -moz-transform:  translateY(0px)  ;
  }
  45% {
    -moz-transform:  translateY(-15px)  ;
  }
  60% {
    -moz-transform:  translateY(0px)  ;
  }
  75% {
    -moz-transform:  translateY(-5px)  ;
  }
  100% {
    -moz-transform:  translateY(0px)  ;
  }
}

@-webkit-keyframes bounce {
  0% {
    -webkit-transform:  translateY(9px)  ;
  }
  15% {
    -webkit-transform:  translateY(-25px)  ;
  }
  30% {
    -webkit-transform:  translateY(0px)  ;
  }
  45% {
    -webkit-transform:  translateY(-15px)  ;
  }
  60% {
    -webkit-transform:  translateY(0px)  ;
  }
  75% {
    -webkit-transform:  translateY(-5px)  ;
  }
  100% {
    -webkit-transform:  translateY(0px)  ;
  }
}

@-o-keyframes bounce {
  0% {
    -o-transform:  translateY(9px)  ;
  }
  15% {
    -o-transform:  translateY(-25px)  ;
  }
  30% {
    -o-transform:  translateY(0px)  ;
  }
  45% {
    -o-transform:  translateY(-15px)  ;
  }
  60% {
    -o-transform:  translateY(0px)  ;
  }
  75% {
    -o-transform:  translateY(-5px)  ;
  }
  100% {
    -o-transform:  translateY(0px)  ;
  }
}

@-ms-keyframes bounce {
  0% {
    -ms-transform:  translateY(9px)  ;
  }
  15% {
    -ms-transform:  translateY(-25px)  ;
  }
  30% {
    -ms-transform:  translateY(0px)  ;
  }
  45% {
    -ms-transform:  translateY(-15px)  ;
  }
  60% {
    -ms-transform:  translateY(0px)  ;
  }
  75% {
    -ms-transform:  translateY(-5px)  ;
  }
  100% {
    -ms-transform:  translateY(0px)  ;
  }
}

@media only screen and (max-width:992px) {
    #byInterestPage #main {
        padding: 20px 20px 0;
    }

    #selectionSummary {
        font-size:20px;
        margin:20px -20px 0;
    }

    .textOverlay p {
        font-size:18px;
    }

    .checkmark {
        height: 50px;
        margin-left: -25px;
    }

    #viewResultsBtn {
        margin:20px 0;
    }

}

@media only screen and (max-width:768px) {

    .textOverlay {
        background-color: rgba(4, 69, 57, 0.6);
    }

    .interestWrapper {
        width: 33.33333333333333%;
        padding:5px;
    }

    #selectionSummary {
        font-size:18px;
    }

    #byInterestPage h2 {
        margin: 20px 20px 10px;
    }

    #viewResultsBtn {
        margin: 20px 0;
        width: initial;
        font-size: 14px;
        padding-right: 34px;
        height: 50px;
        line-height: 50px;
    }

    #viewResultsBtn .fa-angle-right {
        font-size:20px;
        margin-top:-10px;
    }
}


@media only screen and (max-width:600px) {
    .interestWrapper {
       padding: 0 5px 10px;
       width:50%;
    }

    #byInterestPage h2 {
        margin: 10px 10px 20px;
        padding: 0;
    }

    .textOverlay p {
        font-size: 14px;
    }

    .checkmark {
        height: 40px;
        margin-left: -20px;
    }

    #selectionSummary {
        padding:15px 10px;
        font-size:14px;
        margin:0 !important;
    }

    .tryMatchingBtn {
        width: 200px;
        display: none;
        font-size: 12px;
        margin-top:10px;
    }

    #byInterestPage #main {
        padding: 10px 0 0;
    }
}


@media only screen and (max-width:360px) {
    .modal-footer .twoButtons {
        font-size: 11px;
    }
}