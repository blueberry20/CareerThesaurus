	<!-- - - - - - - - - - - - - - Loader - - - - - - - - - - - - - - - - -->	

	<script>
        var allowstickyheader = @(ViewBag.AllowStickyHeader != null && ViewBag.AllowStickyHeader ? "true" : "false");
    </script>
    
	<div class="loader"></div><!--/ .loader-->
	
	
	<!-- - - - - - - - - - - - - end Loader - - - - - - - - - - - - - - - -->	

	
	<!-- - - - - - - - - - - - - - BG Video - - - - - - - - - - - - - - - - -->		

    @if (ViewBag.IncludeBGVideo != null && ViewBag.IncludeBGVideo == true)
    {
	<a class="player" id="bgndVideo" data-property="{
		videoURL: 'http://www.youtube.com/watch?v=o9gLqh8tmPA&feature',
		containment:'body',
		autoPlay: true,
		quality: 'small',
		mute: true, 
		startAt: 0,
