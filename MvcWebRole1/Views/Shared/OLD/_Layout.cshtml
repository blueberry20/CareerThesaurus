<script>

    function submitlockedform() {

        
        if ($.trim($('#lockedfirstname').val()) == '') {
            alert('Please enter your FIRST NAME');
            $('#lockedfirstname').focus();
            return;
        }

        if ($.trim($('#lockedhighestlevel').val()) == "") {
            alert('Please select your level of education');
            $('#lockedhighestlevel').focus();
            return;
        }
        
        if ($.trim($('#lockedzip').val()) == '') {
            alert('Please enter your ZIP');
            $('#lockedzip').focus();
            return;
        }

        var reg = /^\d+$/;
        var enteredzip = $.trim($('#lockedzip').val());
        if (!(reg.test(enteredzip) && enteredzip.length == 5)) {
            alert('Invalid ZIP code');
            $('#lockedzip').focus();
            return;
        }

        if ($.trim($('#lockedphone').val()) == '') {
            alert('Please type your Primary Phone Number \n\n** cell phone is preferred');
            $('#lockedphone').focus();
            return;
        }

        var enteredNumber = $.trim($('#lockedphone').val());
        var formattedNumber = formatPhoneNumber(enteredNumber.replace(/(\s|\.|\-|\(|\))/g, ''));
        if (formattedNumber == "") {
            alert('Invalid phone number');
            $('#lockedphone').focus();
            return;
        }
        else {
            $('#lockedphone').val(formattedNumber);
        }

        if ($.trim($('#lockedemail').val()) == '') {
            alert('Please enter your email');
            $('#lockedemail').focus();
            return;
        }

        if (!validateEmail($.trim($('#lockedemail').val()))) {
            alert('Invalid email: ' + $('#lockedemail').val());
            $('#lockedemail').focus();
            return;
        }

        if ($('#lockedintent').val() == "") {
            alert('Please select your intention');
            $('#lockedintent').focus();
            return;
        }

        setCookie('zip', $('#lockedzip').val(), 365);
        setCookie('firstname', $('#lockedfirstname').val(), 365);
        setCookie('phone', $('#lockedphone').val(), 365);
        setCookie('email', $('#lockedemail').val(), 365);
        setCookie('educationlevel', $('#lockedhighestlevel').val(), 365);
        

        $('#frmlockedregister').submit();

        generatecallbackscript();
    }

