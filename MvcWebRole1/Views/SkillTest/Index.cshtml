                    <tr><td colspan=4 style="height: 5px;">&nbsp;</td></tr>

                    
                    
                </table>

                
            </p>
            <p class="answers" style="text-align: right; bottom: 100px; right: 20px; ">
                <span id="confirmdobyear" style="font-size: 12px; margin: 1px; opacity: 0;">
                    Confirm birth year: 
                    <select id="recordformbirthyear" name="birthyear" style="width: 80px; font-size: 10pt; padding: 5px; margin-right: 20px; height: 30px;">
                        @for (int i = 14; i <= 65; i++)
                        {
                        <option value="@(DateTime.Now.Year-i)">@(DateTime.Now.Year-i)</option>
                        }
                    </select>
                </span>
                <input class="answerbutton" type="button" onclick="saverecordform()" value="Next" style=""/>
                <script>
                    function showconfirmdobyear() {
                        var val = $('#recordformemail').val();
                        if (!validateEmail(val)) {
                            return;
                        }
                        $('#confirmdobyear').css('opacity',1);
                    }
                    function saverecordform() {
                        var val = '';

                        val = $('#recordformsalutation').val();
                        setCookie('salutation', val, 365);

                        val = $('#recordformfirstname').val();
                        if ($.trim(val) == '') {
                            alert('Please enter your first name');
                            $('#recordformfirstname').focus();
                            return;
                        }
                        setCookie('firstname', val, 365);

                        val = $('#recordformlastname').val();
                        if ($.trim(val) == '') {
                            alert('Please enter your last name');
                            $('#recordformlastname').focus();
                            return;
                        }
                        setCookie('lastname', val, 365);
                        
                        var enteredNumber = $.trim($('#recordformphone').val());
                        var formattedNumber = formatPhoneNumber(enteredNumber.replace(/(\s|\.|\-|\(|\))/g, ''));

                        if (formattedNumber == "") {
                            alert('Please enter a valid phone number');
                            $('#recordformphone').focus();
                            return;
                        }
                        else {
                            $('#recordformphone').val(formattedNumber);
                        }
                        setCookie('phone', formattedNumber, 365);

                        val = $('#recordformemail').val();
                        if (!validateEmail(val)) {
                            alert('Please enter a valid email');
                            $('#recordformemail').focus();
                            return;
                        }
                        setCookie('email', val, 365);

                        val = $('#recordformbirthyear').val();
                        setCookie('dobyear', val, 365);

                        $('#basicrecordform').hide();

                        //Stop downloading schools in the background
                        if (!stopdownloadingschoolresults) {
                            finalizeDownloadedSchoolResutls();
                        }

                        try {
                            $.ajax({
                                url: appserverurl + "/skilltest",
                                type: "POST",
                                data: {
                                    sourceform: "record",
                                    zip: getCookie('zip'),
                                    firstname: $('#recordformfirstname').val(),
                                    lastname: $('#recordformlastname').val(),
                                    phone: $('#recordformphone').val(),
                                    email: $('#recordformemail').val(),
                                    educationlevel: getCookie('education_level'),
                                    gradyear: getCookie('gradyear')
                                }
                            });
                        }
                        catch (e) {
                            alert(e.message);
                        }

                        if (getCookie("utm_source") != null && getCookie("utm_source").toLowerCase() == "advertise.com") {
                            firePixel('http://www.advconversion.com/ads-conversiontrack/conversion/track.do?type=img&advertiserId=60431');
                        }

                        if (getCookie("utm_source") != null && getCookie("utm_source").toLowerCase() == "admedia") {
                            fireAdMediaPixel();
                        }

                        setCookie('recordformnextpressed','1');

                        nextQuestion();
                    }
                    
                    
                </script>
            </p>

            <div style="position: absolute; left: 30px; bottom: 20px; width: 555px; font-size: 8pt; color: #555; text-align: left;">
                By entering my information and clicking "Next", I am providing express consent to be contacted by SkillCow.com via email and/or phone, including my wireless phone number, regarding educational opportunities, career services and job openings using automated technology. I also understand that SkillCow.com <a href="javascript:window.open('/theprivacypolicy')" style="font-size: 8pt;" target="_blank">privacy policy</a> governs the collection of this information.
            </div>
            
        </div>
