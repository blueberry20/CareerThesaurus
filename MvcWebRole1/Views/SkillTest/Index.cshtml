                <input class="answerbutton" type="button" onclick="savefilterquestionsform()" value="Next" style=""/>
                <script>
                    function savefilterquestionsform() {
                        val = $('#filterquestionsformhighestlevel').val();
                        if (val == '') {
                            alert('Please select your Highest Level of Education');
                            $('#filterquestionsformhighestlevel').focus();
