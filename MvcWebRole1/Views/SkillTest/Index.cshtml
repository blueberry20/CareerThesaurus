                <input class="answerbutton" type="button" onclick="zipcodeupfront()" value="Next"/>
                <script>
                    function zipcodeupfront() {
                        var val =$('#zipcodeupfrontvalue').val();
                        if (!validateZip(val)) {
                            alert('Please enter your zip code');
                            $('#zipcodeupfrontvalue').focus();
                            return;
                        }
                        setCookie('zip', val, 365);

                        $('#zipcodeupfront').hide();
                        nextQuestion();
                    }
                    $('#zipcodeupfrontvalue').bind('keypress', function (e) {
                        if (e.keyCode == 13) {
                            zipcodeupfront();
                        }
                    });
                </script>
            </p>
        </div>
