<script>

    var profession_name = '';
    var programid = 0;
    var _profession_indepth_loaded = false;

    function showProfessionInDepth(professionid, profession_name, branchname) {

        showOverlay('_profession_indepth');
                
        $('#profession_indepth_branchimage').attr('src', '/content/images/careerassessment/covers/bybranch/' + branchname.replace(/\s/g, '').replace(/\,/g, '') + '.jpg');

        $('#profession_indepth_name').html(profession_name);
        $('#profession_indepth_shortdesc').addClass(professionid + '_description');

        $('#profession_duties').addClass(professionid + '_duties');
        $('#profession_wheretofind').addClass(professionid + '_wheretofind');
        $('#profession_salary').addClass(professionid + '_salary');
        $('#profession_demand').addClass(professionid + '_demand');

        $('#duties_container').addClass(professionid + '_duties_container');
        $('#wheretofind_container').addClass(professionid + '_wheretofind_container');
        $('#salary_container').addClass(professionid + '_salary_container');
        $('#demand_container').addClass(professionid + '_demand_container');
        
        getDescriptionsFromBlob(professionid);
                
        var scrollablewidth = 1920; //125 + 280 + 300 * (cnt - 1) + 1920 - 125 - 280

        $('#_profession_indepth_scrollablecontent').css('width', scrollablewidth);
        $("#_profession_indepth_viewport").niceScroll("#_profession_indepth_scrollablecontent", { cursorcolor: "rgba(0,0,0,0)" });
        $("#_profession_indepth_viewport").getNiceScroll().resize();
        $("#_profession_indepth_viewport").show();

        setTimeout(function () {
            $('#_profession_indepth_scrollablecontent').animate({ 'margin-left': '0px', 'opacity': 1 }, 1500);
        }, 500);

        if (!_profession_indepth_loaded) {
            $('#_profession_indepth_backbutton').press(function () {
                hideOverlay('_profession_indepth');
            });

            $(document).keydown(function (e) {
                if (e.keyCode == 27) {
                    hideOverlay('_profession_indepth');
                }
            });

            _profession_indepth_loaded = true;
        }


    }
    
    

    function addSimilarProfession(sequence, job) {

        $('#profession_similarprofessions').append('<div style="position: relative; display: inline-block; margin-left: 10px; width:280px; height: 274px; ">' +
                        '<div style="position: absolute; top: 0; left: 0; width:280px; height: 274px; border: thin solid black;" class="diagonalwhitegradient80">' +
                        '    <div style="width: 260px; height: 25; text-align: left; font-size: 12pt; font-weight: bold; padding: 10px 10px 0 10px; color: #000; ">' + (linebreak?'BR ':'') + job.jobtitle + '</div>' +
                        '    <div style="width: 260px; height: 25; text-align: left; font-size: 10pt; padding-left: 10px; color: #333;">'+job.company+', '+job.formattedLocation+'</div>' +
                        '    <div id="jobkey' + job.jobkey + '" style="width: 260px; text-align: left; font-size: 10pt; padding: 10px; color: #000;">' + (job.description != null ? job.description : '') + '</div>' +
                        //'    <div timestamp="'+postingdate.getTime()+'" class="age">'+getElapsedTime(postingdate.getTime())+'</div>' + 
                        '</div>' +
                        '<div class="whitereflection" style="position: absolute; top: 284px; left: 1px; width: 280px; height: 50px;"></div>' +
                        '</div>');
    }

    
</script>
