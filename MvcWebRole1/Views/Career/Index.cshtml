                    $(ageboxes[i]).html(getElapsedTime($(ageboxes[i]).attr('timestamp')));
                };
            }, 1000);

            //Blinking dots
            setTimeout(function() {
                $('.dots').css('opacity',0);
                $('.ss').css('color','#000');
                setInterval(function() {
                    $('.dots').css('opacity',0);
                    $('.ss').css('color','#000');
                }, 1000);
            }, 500);
            */
