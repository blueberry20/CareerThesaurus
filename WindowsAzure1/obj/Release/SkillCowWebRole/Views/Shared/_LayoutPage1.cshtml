        <script src="@Url.Content("~/scripts/fileuploader/fileuploader.js?v=" + ViewBag.AppVersion)" type="text/javascript"></script>


        <script type="text/javascript">
            function getBlobData(username, variable) {
                var headID = document.getElementsByTagName("head")[0];
                var newScript = document.createElement('script');
                newScript.type = 'text/javascript';
                var d = new Date();
                newScript.src = '@ViewBag.UrlScheme://chaindate.blob.core.windows.net/' + username + '/data/' + variable + '?d=' + d.getMinutes() + d.getSeconds() + d.getMilliseconds();

                headID.appendChild(newScript);
            }
            function htmlEscape(str) {
                return $('<div/>').text(str).html();
            }
            function htmlEncode(str) {
                return $('<div/>').text(str).html();
            }


            function htmlFromJson(str) {
                return htmlEscape(str.replace(/<br\/>/g, '_br_')).replace(/_br_/g, '<br/>');
            }
        </script>
    </head>

    <body>

        <div class="translucent30 shadow20" style="padding: 10px;">
            <img style="height: 30px;" id="logo" src="@Url.Content("~/Content/Images/Logos/tclogo.png")" onclick="document.location='@Url.Content("~/Home")';" alt="ChainDate logo"/>
            <img style="height: 20px;" id="logotitle" src="@Url.Content("~/Content/Images/Logos/logo-title.png")" onclick="document.location='@Url.Content("~/Home")';" alt="ChainDate"/>
            
        </div>
        

        @RenderBody()
    
