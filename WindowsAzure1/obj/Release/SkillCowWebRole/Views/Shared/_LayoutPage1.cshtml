        <script type="text/javascript">
            var appserverurl = "@ViewBag.AppServerUrl";
            var modernBrowser;
            if (parseInt(navigator.appVersion) >= 5)
                modernBrowser = true;
            else
                modernBrowser = false;
        </script>
    
        <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js?v=" + ViewBag.AppVersion)" type="text/javascript"></script>
