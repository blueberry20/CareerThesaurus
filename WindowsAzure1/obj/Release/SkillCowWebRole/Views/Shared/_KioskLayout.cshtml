        function cookieExists(name) {
            var cookiecount = getCookie('cookiecount');
            if (cookiecount == null || cookiecount == '') {
                cookiecount = 0;
                return false;
            }
            for(var i=0; i<cookiecount; i++) {
                if(getCookie('cookie'+i)==name) {
                    return true;
                }
            }
            return false;
        }

