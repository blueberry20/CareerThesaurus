<script>
    function toggleMenu() {
        if ($('#menu').css('height') == '80px') {
            showMenu();
            return;
        }
        else {
            hideMenu();
            return;
        }
    }

    function showMenu() {
        if ($('#menu').css('height') === '80px') {
            $('#menu').stop();
            $('.menuitems').stop();
            if (!ismobile) {
                $('#menu').addClass('menushadow');
                $('#menu').animate({ 'height': '100%' }, 200);

                setTimeout(function () {
                    $('.menuitems').show();
                    $('.menuitems').animate({ 'opacity': '1' }, 0);
                }, 100);
            }
            else {
                $('#menu').addClass('menushadow');
                $('#menu').animate({ 'width': '250px', 'height': '100%' }, 200);

                setTimeout(function () {
                    $('#menucaption').show();
                    $('.menuitems').show();
                    $('.menuitems').animate({ 'opacity': '1' }, 0);
                }, 200);
            }
        }
    }
    function hideMenu() {
        if ($('#menu').css('height') !== '80px') {
            $('#menu').stop();
            $('.menuitems').stop();
            
            if (!ismobile) {
                $('#menu').removeClass('menushadow');
                $('.menuitems').animate({ 'opacity': '0' }, 200);
                setTimeout(function () {
                    $('.menuitems').hide();
                }, 0);


                $('#menu').animate({ 'height': '80px' }, 200);
            }
            else {
                $('#menucaption').hide();
                $('.menuitems').animate({ 'opacity': '0' }, 200);

                setTimeout(function () {
                    $('.menuitems').hide();
                }, 0);


                $('#menu').animate({ 'height': '80px', 'width': '80px' }, 200);
                setTimeout(function () {
                    $('#menu').removeClass('menushadow');
                }, 200);
            }
        }
    }


