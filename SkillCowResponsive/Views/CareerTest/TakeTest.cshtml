                    setCookie('sport', '', -1);
                    setCookie('school', '', -1);
                    setCookie('color', '', -1);
                    setCookie('currentQuestion', '', -1);
                    setCookie('educationSelected', '', -1);
                    localStorage.removeItem('interestsList');
                    localStorage.removeItem('resultsEmailed');
                });

                $('#continue').click(function (event) {
                    if (!$(this).attr('href')) {
                        event.preventDefault();
                        $('#moveToQuestion').val(nextQuestion);
                        $('#navigationForm').submit();
                    } else {
                        if (ga) ga('send', 'event', 'page', 'Question number', 'Results');
                    }
